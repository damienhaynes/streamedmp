<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<window>
  <id>748</id>
  <defaultcontrol>2</defaultcontrol>
  <allowoverlay>yes</allowoverlay>
  <define>#header.image:television_logo.png</define>
  <define>#header.label:1037</define>
  <define>#header.hover:-</define>
  <define>#selecteditem:</define>
  <controls>
    <import>common.window.tv.xml</import>
    <import>common.time.xml</import>
    <import>common.overlays2.xml</import>
    <control>
      <description>Top Title Background</description>
      <type>image</type>
      <id>0</id>
      <posX>77</posX>
      <posY>11</posY>
      <width>437</width>
      <height>35</height>
      <texture>tvtitlebg.png</texture>
      <animation effect="slide" start="-514,0" end="0,0" tween="quadratic" time="300" delay="0">WindowOpen</animation>
    </control>
    <control>
      <description>Time Background</description>
      <type>image</type>
      <id>0</id>
      <posX>6</posX>
      <posY>10</posY>
      <width>132</width>
      <height>32</height>
      <texture>clockbackground.png</texture>
      <animation effect="fade" time="200" delay="100">WindowOpen</animation>
    </control>
    <control>
      <type>group</type>
      <layout>StackLayout</layout>
      <description>group element</description>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="250">WindowClose</animation>
      <posX>71</posX>
      <posY>220</posY>
      <control>
        <description>Record</description>
        <type>button</type>
        <id>2</id>
        <label />
        <onup>17</onup>
        <ondown>3</ondown>
        <onright>10</onright>
      </control>
      <control>
        <description>Priority</description>
        <type>button</type>
        <id>3</id>
        <label>991</label>
        <onup>2</onup>
        <ondown>4</ondown>
        <onright>10</onright>
      </control>
      <control>
        <description>Keep</description>
        <type>button</type>
        <id>4</id>
        <label>1042</label>
        <onup>3</onup>
        <ondown>5</ondown>
        <onright>10</onright>
      </control>
      <control>
        <description>Only new episodes</description>
        <type>togglebutton</type>
        <id>5</id>
        <label>Only new episodes</label>
        <font>font12</font>
        <textcolor>White</textcolor>
        <colordiffuse>White</colordiffuse>
        <!--<disabledcolor>#60ffffff</disabledcolor>-->
        <onup>4</onup>
        <ondown>8</ondown>
        <onright>10</onright>
      </control>
      <control>
        <description>PreRecord</description>
        <type>button</type>
        <id>8</id>
        <label>1444</label>
        <onup>5</onup>
        <ondown>9</ondown>
        <onright>10</onright>
      </control>
      <control>
        <description>PostRecord</description>
        <type>button</type>
        <id>9</id>
        <label>1445</label>
        <onup>8</onup>
        <ondown>99</ondown>
        <onright>10</onright>
      </control>
    </control>
    <control>
      <description>Scheduled recordings</description>
      <type>label</type>
      <id>11</id>
      <posX>-50</posX>
      <posY>350</posY>
      <label>646</label>
      <align>left</align>
      <font>font13</font>
      <textcolor>FFB2D4F5</textcolor>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="250">WindowClose</animation>
      <visible>false</visible>
    </control>
    <control>
      <description>upcoming episodes listcontrol</description>
      <type>listcontrol</type>
      <id>10</id>
      <posX>520</posX>
      <posY>80</posY>
      <width>800</width>
      <height>460</height>
      <height>362</height>
      <textXOff>88</textXOff>
      <textYOff>2</textYOff>
      <textXOff2>743</textXOff2>
      <textYOff2>36</textYOff2>
      <textXOff3>177</textXOff3>
      <textYOff3>36</textYOff3>
      <IconXOff>21</IconXOff>
      <PinIconXOff>110</PinIconXOff>
      <PinIconYOff>15</PinIconYOff>
      <spinPosX>1031</spinPosX>
      <itemWidth>74</itemWidth>
      <itemHeight>52</itemHeight>
      <!--       <textureFocus>recording_background_focus.png</textureFocus>
      <textureNoFocus>recording_background_nofocus.png</textureNoFocus> -->
      <spaceBetweenItems>8</spaceBetweenItems>
      <textureHeight>62</textureHeight>
      <!--       <textureWidth>138</textureWidth> -->
      <keepaspectratio>no</keepaspectratio>
      <font>mediastream12tc</font>
      <font2>mediastream12</font2>
      <font3>mediastream12</font3>
      <onleft>2</onleft>
      <onright>2</onright>
      <onup>2</onup>
      <ondown>2</ondown>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="250">WindowClose</animation>
    </control>
    <control>
      <description>Current program title</description>
      <type>fadelabel</type>
      <id>13</id>
      <posX>50</posX>
      <posY>505</posY>
      <width>472</width>
      <font>mediastream12tc</font>
    </control>
    <control>
      <description>Current program time</description>
      <type>label</type>
      <id>14</id>
      <posX>50</posX>
      <posY>530</posY>
      <align>left</align>
      <font>mediastream12tc</font>
    </control>
    <control>
      <description>Description1</description>
      <type>textboxscrollup</type>
      <id>15</id>
      <posX>50</posX>
      <posY>555</posY>
      <width>1150</width>
      <height>160</height>
      <label>#TV.RecordedTV.Description</label>
      <font>mediastream11</font>
    </control>
    <control>
      <description>Current program channel</description>
      <type>fadelabel</type>
      <id>16</id>
      <posX>1208</posX>
      <posY>125</posY>
      <width>227</width>
      <align>right</align>
      <font>font13</font>
      <textcolor>FFFFFFFF</textcolor>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="250">WindowClose</animation>
    </control>
    <control>
      <description>genre</description>
      <type>label</type>
      <id>17</id>
      <posX>1275</posX>
      <posY>530</posY>
      <align>right</align>
      <font>mediastream12tc</font>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="250">WindowClose</animation>
    </control>
  </controls>
</window>