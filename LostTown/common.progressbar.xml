<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<window>
  <controls>
    <control>
      <description>progressbar background</description>
      <type>image</type>
      <id>101</id>
      <posX>187</posX>
      <posY>657</posY>
      <width>1044</width>
      <height>24</height>
      <texture>osdprogressback.png</texture>
      <colordiffuse>ffFFFFFF</colordiffuse>
    </control>
    <control>
      <type>progress</type>
      <description>track progress</description>
      <id>1</id>
      <posX>187</posX>
      <posY>663</posY>
      <width>1040</width>
      <height>12</height>
      <label>#percentage</label>
      <texturebg>-</texturebg>
      <lefttexture>osdprogressleft.png</lefttexture>
      <midtexture>osdprogressmid.png</midtexture>
      <righttexture>osdprogressright.png</righttexture>
      <visible>window.isosdvisible|player.paused|player.rewinding|player.forwarding|Control.IsVisible(10)</visible>
    </control>
    <control>
      <description>Start Time Info</description>
      <type>label</type>
      <id>102</id>
      <posX>195</posX>
      <posY>679</posY>
      <label>#currentplaytime</label>
      <align>left</align>
      <font>mediastream12c</font>
      <textcolor>ff888888</textcolor>
    </control>
    <control>
      <description>End Time Info</description>
      <type>label</type>
      <id>103</id>
      <posX>1221</posX>
      <posY>679</posY>
      <label>#currentremaining</label>
      <align>right</align>
      <font>mediastream12c</font>
      <textcolor>ff888888</textcolor>
    </control>
  </controls>
</window>