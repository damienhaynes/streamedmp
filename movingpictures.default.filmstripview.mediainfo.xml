<?xml version="1.0" encoding="utf-8"?>
<!-- MediaInfo Logos (Filmstrip Thumbs View) -->
<window>
  <controls>
    <!--Video Resolutions / Aspect Ratio-->
    <control>
      <description>720p, 1080i, 1080p</description>
      <type>image</type>
      <id>9801</id>
      <posX>1105</posX>
      <posY>110</posY>
      <width>100</width>
      <height>32</height>
      <texture>Logos\Video\#MovingPictures.LocalMedia.videoresolution.png</texture>
      <visible>control.isvisible(50)+facadeview.filmstrip</visible>
			<animation effect="fade" time="250" delay="0">WindowClose</animation>
    </control>
    <control>
      <description>SD/HD Fullscreen/Widescreen</description>
      <type>image</type>
      <id>0</id>
      <posX>1105</posX>
      <posY>110</posY>
      <width>100</width>
      <height>32</height>
      <texture>Logos\Video\#MovingPictures.LocalMedia.videoresolution #MovingPictures.LocalMedia.videoaspectratio.png</texture>
      <visible>control.isvisible(50)+facadeview.filmstrip+!Control.HasThumb(9801)</visible>
			<animation effect="fade" time="250" delay="0">WindowClose</animation>
    </control>
    <!-- Video Codecs -->
    <control>
      <description>H264</description>
      <type>image</type>
      <id>0</id>
      <posX>1105</posX>
      <posY>144</posY>
      <width>100</width>
      <height>32</height>
      <texture>Logos\Video\#MovingPictures.LocalMedia.videocodec.png</texture>
      <visible>control.isvisible(50)+facadeview.filmstrip</visible>
			<animation effect="fade" time="250" delay="0">WindowClose</animation>
    </control>
    <!-- Audio Channels / Codecs -->
    <control>
      <description>Audio Codecs / Channels</description>
      <type>image</type>
      <id>9802</id>
      <posX>1105</posX>
      <posY>178</posY>
      <width>100</width>
      <height>32</height>
      <texture>Logos\Audio\#MovingPictures.LocalMedia.audiocodec #MovingPictures.LocalMedia.audiochannels.png</texture>
      <visible>control.isvisible(50)+facadeview.filmstrip</visible>
			<animation effect="fade" time="250" delay="0">WindowClose</animation>
    </control>
    <control>
      <description>Audio Codecs - Unknown number of channels</description>
      <type>image</type>
      <id>0</id>
      <posX>1105</posX>
      <posY>178</posY>
      <width>100</width>
      <height>32</height>
      <texture>Logos\Audio\#MovingPictures.LocalMedia.audiocodec.png</texture>
      <visible>control.isvisible(50)+facadeview.filmstrip+!Control.HasThumb(9802)</visible>
			<animation effect="fade" time="250" delay="0">WindowClose</animation>
    </control>
    <!-- Subtitles -->
    <control>
      <description>Subtitles</description>
      <type>image</type>
      <id>0</id>
      <posX>1105</posX>
      <posY>212</posY>
      <width>100</width>
      <height>32</height>
      <texture>Logos\Misc\#MovingPictures.LocalMedia.Subtitles.png</texture>
      <visible>control.isvisible(50)+facadeview.filmstrip</visible>
			<animation effect="fade" time="250" delay="0">WindowClose</animation>
    </control>
  </controls>
</window>
