<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<window>
  <id>600</id>
  <defaultcontrol>2</defaultcontrol>
  <allowoverlay>no</allowoverlay>
  <controls>
    <import>common.time.xml</import>
    <control>
      <description>BG</description>
      <type>image</type>
      <id>0</id>
      <width>1920</width>
      <height>1080</height>
      <texture>hover_my tv.png</texture>
    </control>
    <control>
      <type>image</type>
      <posY>0</posY>
      <posX>0</posX>
      <height>1080</height>
      <width>1920</width>
      <texture>tvguidebg.png</texture>
    </control>
    <control>
      <description>Top Title Background</description>
      <type>image</type>
      <id>0</id>
      <posX>115</posX>
      <posY>16</posY>
      <width>655</width>
      <height>52</height>
      <texture>tvtitlebg.png</texture>
      <animation effect="slide" start="-771,0" end="0,0" tween="quadratic" time="300" delay="0">WindowOpen</animation>
      <animation effect="fade" time="600">WindowOpen</animation>
    </control>
    <control>
      <type>fadelabel</type>
      <description>Top Title</description>
      <id>0</id>
      <posX>739</posX>
      <posY>19</posY>
      <width>525</width>
      <font>mediastream12c</font>
      <align>right</align>
      <visible>yes</visible>
      <label>#currentmodule</label>
      <animation effect="slide" start="-771,0" end="0,0" tween="quadratic" time="300" delay="0">WindowOpen</animation>
      <animation effect="fade" time="400">WindowOpen</animation>
    </control>
    <control>
      <description>Top Title Extension Background</description>
      <type>image</type>
      <id>0</id>
      <posX>646</posX>
      <posY>16</posY>
      <width>684</width>
      <height>54</height>
      <texture>tvtitlebg.png</texture>
      <animation effect="fade" time="300" delay="300">WindowOpen</animation>
      <animation effect="slide" start="-771,0" end="0,0" tween="quadratic" time="300" delay="300">WindowOpen</animation>
    </control>
    <control>
      <type>label</type>
      <description>Guide Group Label</description>
      <id>11</id>
      <posX>1278</posX>
      <posY>19</posY>
      <width>510</width>
      <font>mediastream12c</font>
      <align>right</align>
      <label>#TV.Guide.Group</label>
      <visible>no</visible>
    </control>
    <control>
      <type>label</type>
      <description>Guide Group Label</description>
      <id>0</id>
      <posX>1278</posX>
      <posY>19</posY>
      <width>510</width>
      <font>mediastream12c</font>
      <align>right</align>
      <label>#TV.Guide.ChangeGroup</label>
      <animation effect="fade" time="300" delay="300">WindowOpen</animation>
      <animation effect="slide" start="-771,0" end="0,0" tween="bounce" time="700" delay="300">WindowOpen</animation>
    </control>
    <control>
      <description>Time Background</description>
      <type>image</type>
      <id>0</id>
      <posX>9</posX>
      <posY>15</posY>
      <width>198</width>
      <height>48</height>
      <texture>clockbackground.png</texture>
    </control>
    <control>
      <description>Date</description>
      <type>label</type>
      <id>0</id>
      <posX>1845</posX>
      <posY>45</posY>
      <label>#date</label>
      <align>right</align>
      <font>mediastream11tc</font>
    </control>
    <control>
      <description>TVGUIDE Panel</description>
      <type>image</type>
      <id>2</id>
      <posX>30</posX>
      <posY>172</posY>
      <width>1860</width>
      <height>675</height>
      <colordiffuse>ffffffff</colordiffuse>
      <texture>-</texture>
    </control>
    <control>
      <description>Single Channel Label</description>
      <type>label</type>
      <id>32</id>
      <posX>1278</posX>
      <posY>19</posY>
      <width>450</width>
      <font>mediastream12c</font>
      <align>right</align>
      <animation effect="fade" time="300" delay="300">Visible</animation>
      <animation effect="slide" start="-771,0" end="0,0" tween="bounce" time="700" delay="300">Visible</animation>
    </control>
    <control>
      <description>Single Channel Image</description>
      <type>image</type>
      <id>33</id>
      <posX>457</posX>
      <posY>103</posY>
      <width>67</width>
      <height>67</height>
      <keepaspectratio>yes</keepaspectratio>
      <zoom>no</zoom>
      <centered>yes</centered>
      <texture>defaultVideoBig.png</texture>
      <animation effect="slide" start="1920,0" end="0,0" tween="circle" time="1700">Visible</animation>
      <animation effect="fade" time="300">Visible</animation>
    </control>
    <!-- Timeheader background must be dedfined before Timeheader label template -->
    <!-- b/c Timeheader is on the background and will be rendered first -->
    <control>
      <description>time header background image template</description>
      <type>image</type>
      <id>90</id>
      <posX>355</posX>
      <posY>105</posY>
      <width>244</width>
      <height>51</height>
      <texture>button_small_settings_nofocus.png</texture>
    </control>
    <control>
      <description>time header label template</description>
      <type>label</type>
      <id>40</id>
      <posX>457</posX>
      <posY>105</posY>
      <font>mediastream11tc</font>
    </control>
    <control>
      <description>day spin control</description>
      <type>spincontrol</type>
      <id>6</id>
      <width>52</width>
      <height>52</height>
      <posX>73</posX>
      <posY>97</posY>
      <showrange>no</showrange>
      <orientation>horizontal</orientation>
      <align>center</align>
      <onleft>6</onleft>
      <onright>8</onright>
      <textureUp>arrow_round_right_nofocus.png</textureUp>
      <textureDown>arrow_round_left_nofocus.png</textureDown>
      <textureUpFocus>arrow_round_right_focus.png</textureUpFocus>
      <textureDownFocus>arrow_round_left_focus.png</textureDownFocus>
    </control>
    <control>
      <description>time interval spin control</description>
      <type>spincontrol</type>
      <id>8</id>
      <width>52</width>
      <height>52</height>
      <posX>322</posX>
      <posY>97</posY>
      <showrange>no</showrange>
      <orientation>horizontal</orientation>
      <align>center</align>
      <textureUp>arrow_round_right_nofocus.png</textureUp>
      <textureDown>arrow_round_left_nofocus.png</textureDown>
      <textureUpFocus>arrow_round_right_focus.png</textureUpFocus>
      <textureDownFocus>arrow_round_left_focus.png</textureDownFocus>
      <onleft>6</onleft>
      <onright>8</onright>
    </control>
    <import tag="TVGuideChannelTemplate">mytvguide.common.10rows.channeltemplate.xml</import>
    <control>
      <description>record pin</description>
      <type>image</type>
      <id>31</id>
      <posX>-2198</posX>
      <posY>880</posY>
      <width>67</width>
      <height>25</height>
      <texture>tvguide_record_button.png</texture>
      <keepaspectratio>yes</keepaspectratio>
      <visible>no</visible>
    </control>
    <control>
      <description>Current program title</description>
      <type>fadelabel</type>
      <id>0</id>
      <posX>360</posX>
      <posY>877</posY>
      <width>1200</width>
      <label>#TV.Guide.CompositeTitle</label>
      <font>mediastream11tc</font>
    </control>
    <control>
      <description>Current program time</description>
      <type>label</type>
      <id>0</id>
      <posX>1875</posX>
      <posY>877</posY>
      <width>330</width>
      <align>right</align>
      <label>#TV.Guide.Time</label>
      <font>mediastream11tc</font>
    </control>
    <control>
      <description>Description</description>
      <type>textboxscrollup</type>
      <id>0</id>
      <posX>360</posX>
      <posY>915</posY>
      <width>1530</width>
      <height>105</height>
      <seperator>________________________________________________________________________________________________________________</seperator>
      <label>#TV.Guide.Description</label>
      <font>mediastream11</font>
      <textcolor>FFFFFFFF</textcolor>
    </control>
    <control>
      <description>vertical timeline</description>
      <type>image</type>
      <id>25</id>
      <posX>82</posX>
      <posY>145</posY>
      <height>712</height>
      <colordiffuse>c0ffffff</colordiffuse>
      <texture>tvguide_slider.png</texture>
    </control>
    <control>
      <description>TvGroup button label</description>
      <type>label</type>
      <id>0</id>
      <posX>60</posX>
      <posY>202</posY>
      <label>#TV.Guide.ChangeGroup</label>
      <font>mediastream12c</font>
      <textcolor>FFFFFFFF</textcolor>
      <animation effect="rotate" end="-90" time="0">WindowOpen</animation>
    </control>
    <control>
      <description>TV background</description>
      <type>image</type>
      <id>98</id>
      <posX>15</posX>
      <posY>870</posY>
      <width>330</width>
      <height>195</height>
      <texture>tv_frame.png</texture>
      <visible>Player.HasVideo</visible>
    </control>
    <control>
      <description>video preview window</description>
      <type>videowindow</type>
      <id>99</id>
      <posX>30</posX>
      <posY>892</posY>
      <width>300</width>
      <height>157</height>
      <action>18</action>
      <textureFocus>tv_green_border.png</textureFocus>
      <visible>Player.HasVideo</visible>
    </control>
    <include condition="#(not(skin.hassetting('#skin.tvguide.usecolorsforbuttons')))">mytvguide.nocolors.xml</include>
    <include condition="#(skin.hassetting('#skin.tvguide.usecolorsforbuttons'))">mytvguide.colors.xml</include>
  </controls>
</window>
