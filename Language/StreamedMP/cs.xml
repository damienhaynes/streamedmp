  <string name="SettingPluginEnabledName">Modul je povolen</string>
  <string name="SettingPluginEnabledDescription">Toto nastavení kontroluje, zda je modul Trakt načten po spuštění aplikace MediaPortal.</string>
  <string name="SettingListedHomeName">Zobrazit na úvodní obrazovce</string>
  <string name="SettingListedHomeDescription">Při povolení se modul Trakt zobrazí na hlavním panelu aplikace MediaPortal.</string>
