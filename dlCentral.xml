<?xml version="1.0" encoding="utf-8"?>
<window>
  <id>181966</id>
  <defaultcontrol>912</defaultcontrol>
  <allowoverlay>no</allowoverlay>
  <!-- Colour when exact match is found -->
  <define>#selectedcolor:ffEB9E17</define>
  <controls>
    <!-- 
		This is the main dlCentral skin file and should contain all screen elements that are common
		to all views in the GUI. Anything that belongs only in a specific view should be placed in the
		coorespodning subfile.
		-->
    <!-- Dummy Controls meant for visibility and status purposes. -->
    <control>
      <description>Visible when in the main/about view mode.</description>
      <type>label</type>
      <id>101</id>
      <posX>-75</posX>
      <posY>-75</posY>
      <width>0</width>
    </control>
    <control>
      <description>Visible when in the search mode.</description>
      <type>label</type>
      <id>102</id>
      <posX>-75</posX>
      <posY>-75</posY>
      <width>0</width>
    </control>
    <control>
      <description>Visible when in the status mode.</description>
      <type>label</type>
      <id>103</id>
      <posX>-75</posX>
      <posY>-75</posY>
      <width>0</width>
    </control>
    <!-- Backgrounds -->
    <control>
      <description>Default Background</description>
      <type>image</type>
      <id>0</id>
      <posX>0</posX>
      <posY>0</posY>
      <width>1920</width>
      <height>1080</height>
      <texture>hover_my plugins.png</texture>
      <animation effect="fade" time="250" delay="0">WindowOpen</animation>
    </control>
    <control>
      <description>backdrop</description>
      <type>image</type>
      <id>0</id>
      <posX>0</posX>
      <posY>0</posY>
      <width>1920</width>
      <height>1080</height>
      <texture>#dlCentral.Search.Media.FanArt</texture>
      <animation effect="fade" time="1">WindowClose</animation>
    </control>
    <control>
      <description>Fanart Overlay</description>
      <type>image</type>
      <id>0</id>
      <posX>-3</posX>
      <posY>0</posY>
      <width>1926</width>
      <height>1083</height>
      <texture>mainoverlay.png</texture>
      <visible>!control.isvisible(103)</visible>
    </control>
    <control>
      <description>Top Bar Overlay</description>
      <type>image</type>
      <id>0</id>
      <posX>0</posX>
      <posY>0</posY>
      <width>1920</width>
      <height>76</height>
      <texture>overlay_topbar.png</texture>
      <visible>control.isvisible(103)</visible>
    </control>
    <!-- Title Area -->
    <control>
      <description>Top Title Extension Background</description>
      <type>image</type>
      <id>0</id>
      <posX>646</posX>
      <posY>16</posY>
      <width>684</width>
      <height>54</height>
      <texture>tvtitlebg.png</texture>
      <visible>control.isvisible(102)+!string.equals(#dlCentral.Search.SearchType.Type,NONE)</visible>
      <animation effect="slide" start="-771,0" end="0,0" tween="quadratic" time="300" delay="300">Visible</animation>
      <animation effect="fade" time="250" delay="0">WindowOpen</animation>
      <animation effect="fade" time="250" delay="0">WindowClose</animation>
    </control>
    <control>
      <description>Top Title Background</description>
      <type>image</type>
      <id>0</id>
      <posX>115</posX>
      <posY>16</posY>
      <width>655</width>
      <height>52</height>
      <texture>tvtitlebg.png</texture>
      <animation effect="slide" start="-771,0" end="0,0" tween="quadratic" time="300" delay="300">Visible</animation>
      <animation effect="slide" start="0,0" end="-771,0" tween="quadratic" time="600" delay="0">Hidden</animation>
      <animation effect="fade" time="250" delay="0">WindowOpen</animation>
      <animation effect="fade" time="250" delay="0">WindowClose</animation>
    </control>
    <control>
      <type>fadelabel</type>
      <description>Top Title</description>
      <id>0</id>
      <posX>739</posX>
      <posY>19</posY>
      <width>525</width>
      <font>mediastream12c</font>
      <align>right</align>
      <label>#dlCentral.Header.Label</label>
      <animation effect="slide" start="-771,0" end="0,0" tween="quadratic" time="300" delay="300">Visible</animation>
      <animation effect="slide" start="0,0" end="-771,0" tween="quadratic" time="600" delay="0">Hidden</animation>
      <animation effect="fade" time="250" delay="0">WindowOpen</animation>
      <animation effect="fade" time="250" delay="0">WindowClose</animation>
    </control>
    <!-- Controls common to all views of the GUI -->
    <import>common.time.xml</import>
    <!-- Imports for various views of the GUI -->
    <import>dlCentral.Search.xml</import>
    <import>dlCentral.Main.xml</import>
    <!--     <import>dlCentral.Status.xml</import>   -->
    <import>dlCentral.HiddenMenu.xml</import>
    <!-- Video / Music overlay -->
    <import>common.overlays.xml</import>
  </controls>
</window>
