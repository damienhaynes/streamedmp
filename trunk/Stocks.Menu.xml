        <description>Switch</description>
        <type>button</type>
        <id>2</id>
        <label>Switch</label>
        <onleft>2</onleft>
        <onright>100</onright>
        <onup>4</onup>
        <ondown>3</ondown>
        <width>250</width>
      </control>
      <control>
        <description>Switch View</description>
        <type>button</type>
        <id>3</id>
        <label>Switch View</label>
        <onleft>3</onleft>
        <onright>3</onright>
        <onup>2</onup>
        <ondown>4</ondown>
