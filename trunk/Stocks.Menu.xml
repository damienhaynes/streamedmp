<?xml version="1.0" encoding="utf-8"?>
<window>
  <controls>
    <control>
      <description>Menu Title</description>
      <id>0</id>
      <posX>20</posX>
      <posY>240</posY>
      <label>924</label>
      <type>label</type>
      <font>mediastream12tc</font>
      <height>25</height>
      <animation effect="fade" time="300">WindowOpen</animation>
      <animation effect="fade" time="300">WindowClose</animation>
    </control>
    <control>
      <type>image</type>
      <posX>0</posX>
      <posY>216</posY>
