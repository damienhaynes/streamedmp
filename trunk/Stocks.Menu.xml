        <onleft>4</onleft>
        <onright>4</onright>
        <onup>3</onup>
        <ondown>5</ondown>
        <width>250</width>
      </control>
      <control>
        <description>Refresh</description>
        <type>button</type>
        <id>5</id>
        <label>Refresh</label>
        <onleft>5</onleft>
        <onright>5</onright>
        <onup>4</onup>
        <ondown>2</ondown>
        <width>250</width>
      </control>
    </control>
  </controls>
