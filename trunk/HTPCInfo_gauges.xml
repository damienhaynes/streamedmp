      <posY>250</posY>
      <width>145</width>
      <height>120</height>
      <keepaspectratio>yes</keepaspectratio>
      <texture>#HTPCInfo.ProcessorGauge</texture>
      <visible>string.equals(#HTPCInfo.SectionView, Gauges)</visible>
      <animation effect="fade" time="500">visiblechange</animation>
