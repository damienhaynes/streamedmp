<?xml version="1.0" encoding="utf-8"?>
<!-- MediaInfo Logos (Small/Large Thumbs View) -->
<window>
  <controls>
    <!-- Video Resolutions / Aspect Ratio-->
    <control>
      <description>720p, 1080i, 1080p</description>
      <type>image</type>
      <id>97993</id>
      <posX>52</posX>
      <posY>945</posY>
      <width>150</width>
      <height>48</height>
      <texture flipY="true" diffuse="Thumb_Mask.png">Logos\MediaInfo\#StreamedMP.MediaInfo.Type\Video\#MovingPictures.LocalMedia.videoresolution.png</texture>
      <visible>control.isvisible(50)+facadeview.smallicons</visible>
      <animation effect="fade" time="250" delay="0">WindowClose</animation>
    </control>
