<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<window>
  <id>5002</id>
  <defaultcontrol>3</defaultcontrol>
  <allowoverlay>yes</allowoverlay>
  <define>#header.label:859</define>
  <define>#header.hover:hover_my alarm.png</define>
  <controls>
    
    <import>common.alarm.xml</import>
    <import>common.time.xml</import>
    
    <control Style="settingsmaintexturefo">
      <type>image</type>
      <posY>269</posY>
      <visible>control.hasfocus(4)</visible>
    </control>
    <control Style="settingsmaintexturenf">
      <type>image</type>
      <posY>269</posY>
      <visible>!control.hasfocus(4)</visible>
    </control>
    <control Style="settingsmaintexturefo">
      <type>image</type>
      <posY>311</posY>
      <visible>control.hasfocus(5)</visible>
    </control>
    <control Style="settingsmaintexturenf">
      <type>image</type>
      <posY>311</posY>
      <visible>!control.hasfocus(5)</visible>
    </control>
    <control Style="settingsmaintexturefo">
      <type>image</type>
      <posY>353</posY>
      <visible>control.hasfocus(6)</visible>
    </control>
    <control Style="settingsmaintexturenf">
      <type>image</type>
      <posY>353</posY>
      <visible>!control.hasfocus(6)</visible>
    </control>
    
    <control>
      <type>group</type>
      <description>group element</description>
      <animation effect="fade" time="200" delay="200">windowopen</animation>
      <posX>251</posX>
      <posY>185</posY>
      <width>595</width>
      <height>500</height>
      <dimColor>ffFFFFFF</dimColor>
      <layout>StackLayout</layout>
      
      <control Style="settingsmaintogglebutton">
        <description>SleepTimer On/Off button</description>
        <type>togglebutton</type>
        <id>3</id>
        <label>856</label>
        <onup>17</onup>
        <ondown>7</ondown>
      </control>
      
      <control Style="settingsmainbutton">
        <description>Reset Button</description>
        <type>button</type>
        <id>7</id>
        <label>868</label>
        <onup>3</onup>
        <ondown>4</ondown>
      </control>
      
    </control>
    
    <control>
      <description>sleep time label</description>
      <type>label</type>
      <id>0</id>
      <posX>251</posX>
      <posY>110</posY>
      <label>CURRENT:</label>
      <align>left</align>
      <font>mediastream14c</font>
      <textcolor>FFffffff</textcolor>
    </control>
    <control>
      <description>sleep time label</description>
      <type>label</type>
      <id>0</id>
      <posX>747</posX>
      <posY>110</posY>
      <label>#currentsleeptime</label>
      <align>left</align>
      <font>mediastream14c</font>
      <textcolor>FFffffff</textcolor>
    </control>
    
    <control>
      <description>Number of minutes until sleep</description>
      <type>label</type>
      <id>1</id>
      <posX>251</posX>
      <posY>277</posY>
      <label>865</label>
      <font>mediastream12c</font>
    </control>
    <control>
      <description>alarm time minutes</description>
      <type>spincontrol</type>
      <spintype>int</spintype>
      <orientation>vertical</orientation>
      <digits>2</digits>
      <id>4</id>
      <posX>800</posX>
      <posY>283</posY>
      <showrange>no</showrange>
      <onup>7</onup>
      <ondown>5</ondown>
    </control>
    
    <control Style="settingsmaincheckmark">
      <description>fade out volume</description>
      <type>checkmark</type>
      <id>5</id>
      <posY>319</posY>
      <label>866</label>
      <onup>4</onup>
      <ondown>6</ondown>
    </control>
    
    <control Style="settingsmaincheckmark">
      <description>shutdown pc</description>
      <type>checkmark</type>
      <id>6</id>
      <posY>361</posY>
      <label>867</label>
      <onup>5</onup>
      <ondown>3</ondown>
    </control>
    
  </controls>
</window>