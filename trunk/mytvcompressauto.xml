  <controls>
    <import>common.window.tv.xml</import>
    <import>common.time.xml</import>
    <control>
      <description>Enable auto compress</description>
      <type>checkmark</type>
      <id>2</id>
      <posX>310</posX>
      <posY>100</posY>
      <label>994</label>
      <onleft>2</onleft>
      <onright>2</onright>
      <onup>17</onup>
      <ondown>5</ondown>
    </control>
    <control>
      <description>Top Title Background</description>
      <type>image</type>
      <id>0</id>
      <posX>77</posX>
      <posY>11</posY>
      <width>437</width>
      <height>35</height>
