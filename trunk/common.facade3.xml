<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<window>
  <controls>
    <control>
      <description>group element</description>
      <type>group</type>
      <control>
        <description>composite control consisting of a list control and a thumbnail panel</description>
        <type>facadeview</type>
        <id>50</id>
        <control>
          <description>listcontrol</description>
          <type>listcontrol</type>
          <id>50</id>
          <scrollOffset>2</scrollOffset>
        </control>
        <control>
          <description>album view listcontrol</description>
          <type>listcontrol</type>
          <subtype>album</subtype>
          <id>50</id>
          <font>font12</font>
          <font2>font12</font2>
          <selectedColor>FFFF6600</selectedColor>
          <textcolor>ffffffff</textcolor>
          <selectedColor2>FFFF6600</selectedColor2>
          <textcolor2>ffffffff</textcolor2>
          <textXOff>15</textXOff>
          <textYOff>15</textYOff>
          <textXOff2>1030</textXOff2>
          <textYOff2>45</textYOff2>
          <itemWidth>72</itemWidth>
          <itemHeight>72</itemHeight>
          <spaceBetweenItems>0</spaceBetweenItems>
          <textureHeight>96</textureHeight>
          <scrollOffset>1</scrollOffset>
          <textureFocus></textureFocus>
          <textureNoFocus>album_background_nofocus.png</textureNoFocus>
          <spinPosY>560</spinPosY>
          <onleft>2</onleft>
          <onright>50</onright>
          <onup>2</onup>
          <ondown>2</ondown>
        </control>
        <control>
          <description>Thumbnail Panel</description>
          <type>thumbnailpanel</type>
          <id>50</id>
          <posX>310</posX>
          <posY>230</posY>
          <width>750</width>
          <height>400</height>
          <spinWidth>16</spinWidth>
          <spinHeight>16</spinHeight>
          <spinPosY>590</spinPosY>
          <spinPosX>1174</spinPosX>
          <spinColor>ffffffff</spinColor>
          <textureUp>arrowupnf.png</textureUp>
          <textureDown>arrowdownnf.png</textureDown>
          <textureUpFocus>arrowupfo.png</textureUpFocus>
          <textureDownFocus>arrowdownfo.png</textureDownFocus>
          <itemWidth>100</itemWidth>
          <itemHeight>79</itemHeight>
          <textureWidth>100</textureWidth>
          <textureHeight>79</textureHeight>
          <thumbWidth>80</thumbWidth>
          <thumbHeight>58</thumbHeight>
          <thumbPosX>8</thumbPosX>
          <thumbPosY>8</thumbPosY>
          <thumbZoom>no</thumbZoom>
          <imageFolder>pic_preview_thumb_background_nofocus.png</imageFolder>
          <imageFolderFocus>pic_preview_thumb_background_focus.png</imageFolderFocus>
          <font>font12</font>
          <selectedColor>00ffffff</selectedColor>
          <suffix>|</suffix>
          <textureWidthBig>164</textureWidthBig>
          <textureHeightBig>129</textureHeightBig>
          <itemWidthBig>164</itemWidthBig>
          <itemHeightBig>129</itemHeightBig>
          <thumbWidthBig>142</thumbWidthBig>
          <thumbHeightBig>105</thumbHeightBig>
          <thumbPosXBig>8</thumbPosXBig>
          <thumbPosYBig>8</thumbPosYBig>
          <thumbZoom>no</thumbZoom>
          <enableFocusZoom>no</enableFocusZoom>
          <onleft>2</onleft>
          <onright>50</onright>
          <onup>2</onup>
          <ondown>2</ondown>
        </control>
        <control>
          <description>Filmstrip view</description>
          <type>filmstrip</type>
          <id>50</id>
          <posX>70</posX>
          <posY>210</posY>
          <width>1220</width>
          <onup>2</onup>
          <spinPosY>550</spinPosY>
          <spinPosX>1150</spinPosX>
          <thumbWidth>200</thumbWidth>
          <thumbHeight>200</thumbHeight>
          <thumbPosX>5</thumbPosX>
          <thumbPosY>5</thumbPosY>
          <imageFolderFocus>Framefocus.png</imageFolderFocus>
          <frame>-</frame>
          <frameFocus>music_filmstrip_frame.png</frameFocus>
          <itemWidth>202</itemWidth>
          <itemHeight>210</itemHeight>
          <textureWidth>220</textureWidth>
          <textureHeight>220</textureHeight>
          <background>Coverbig.png</background>
          <thumbs flipY="true" diffuse="Thumb_Mask.png"></thumbs>
          <InfoImage flipY="true" diffuse="Thumb_Mask.png"></InfoImage>
          <showFrame>yes</showFrame>
          <showFolder>no</showFolder>
          <showBackGround>no</showBackGround>
          <showInfoImage>no</showInfoImage>
          <thumbAnimation effect="zoom" start="100,100" end="130,130" time="300">focus</thumbAnimation>
          <thumbAnimation effect="zoom" start="130,130" end="100,100" time="300">unfocus</thumbAnimation>
        </control>
      </control>
    </control>
  </controls>
</window>
