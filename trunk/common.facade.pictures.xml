<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<window>
  <controls>
    <control>
      <description>group element</description>
      <type>group</type>
      <control>
        <description>composite control consisting of a list control and a thumbnail panel</description>
        <type>facadeview</type>
        <id>50</id>
        <control>
          <description>listcontrol</description>
          <type>listcontrol</type>
          <id>50</id>
          <onleft>2</onleft>
          <onright>2</onright>
          <spinPosX>1249</spinPosX>
          <spinPosY>690</spinPosY>
          <spinAlign>Right</spinAlign>
          <spinColor>ff006698</spinColor>
          <spinCanFocus>no</spinCanFocus>
        </control>
        <control>
          <description>Thumbnail Panel</description>
          <type>thumbnailpanel</type>
          <id>50</id>
          <onleft>2</onleft>
          <onright>2</onright>
          <!--<unfocusedAlpha>200</unfocusedAlpha>-->
        </control>
        <control>
          <description>Filmstrip view</description>
          <type>filmstrip</type>
          <id>50</id>
          <onup>2</onup>
          <posX>26</posX>
          <posY>513</posY>
          <width>1280</width>
          <spinPosY>1300</spinPosY>
          <spinPosX>1218</spinPosX>
          <thumbWidth>140</thumbWidth>
          <thumbHeight>140</thumbHeight>
          <thumbPosX>7</thumbPosX>
          <thumbPosY>7</thumbPosY>
          <itemWidth>179</itemWidth>
          <itemHeight>154</itemHeight>
          <textureWidth>154</textureWidth>
          <textureHeight>154</textureHeight>
          <imageFolderFocus>imagebg.png</imageFolderFocus>
          <imageFolder>imagebg.png</imageFolder>
          <frame>imagebg.png</frame>
          <frameFocus>imagebg.png</frameFocus>
          <background>imagebg.png</background>
          <thumbs />
          <InfoImage />
          <showFrame>no</showFrame>
          <showFolder>yes</showFolder>
          <showBackGround>no</showBackGround>
          <showInfoImage>no</showInfoImage>
          <thumbAnimation effect="zoom" start="100,100" end="133,133" time="0">focus</thumbAnimation>
          <thumbAnimation effect="zoom" start="133,133" end="100,100" time="0">unfocus</thumbAnimation>
          <thumbAnimation effect="slide" end="0,-18" time="0">focus</thumbAnimation>
          <thumbAnimation effect="slide" end="0,0" time="0">unfocus</thumbAnimation>
          <scrollOffset>3</scrollOffset>
        </control>
        <control>
          <description>playlist listcontrol</description>
          <type>playlistcontrol</type>
          <id>50</id>
          <onleft>2</onleft>
          <onright>2</onright>
          <scrollOffset>3</scrollOffset>
          <spinPosX>1249</spinPosX>
          <spinPosY>690</spinPosY>
          <spinAlign>Right</spinAlign>
          <spinColor>ff006698</spinColor>
          <spinCanFocus>no</spinCanFocus>
        </control>
      </control>
    </control>
    
    <control>
      <description>Media Count Background</description>
      <type>image</type>
      <id>0</id>
      <posX>1113</posX>
      <posY>684</posY>
      <width>159</width>
      <height>28</height>
      <texture>mediacountbackground.png</texture>
      <visible>facadeview.list|facadeview.playlist</visible>
    </control>
    
    <control>
      <description>Media Count Label</description>
      <type>label</type>
      <label>PAGE:</label>
      <id>0</id>
      <posX>1123</posX>
      <posY>688</posY>
      <font>mediastream10tc</font>
      <textcolor>ff909090</textcolor>
      <visible>facadeview.list|facadeview.playlist</visible>
    </control>
    
  </controls>
</window>