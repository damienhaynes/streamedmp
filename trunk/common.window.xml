<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<window>
  <controls>
    <!--
    WindowClose animations cause images not to become visible with tvseries and moving pictures plugins
    -->
    <control>
      <description>Top Title Background</description>
      <type>image</type>
      <id>0</id>
      <posX>115</posX>
      <posY>16</posY>
      <width>655</width>
      <height>52</height>
      <texture>tvtitlebg.png</texture>
      <animation effect="slide" start="-771,0" end="0,0" tween="quadratic" time="300" delay="0">WindowOpen</animation>
    </control>
    <control>
      <type>fadelabel</type>
      <description>Top Title</description>
      <id>0</id>
      <posX>739</posX>
      <posY>19</posY>
      <width>525</width>
      <font>mediastream12c</font>
      <align>right</align>
      <label>#(iif(eq('#currentmodule',''),'#header.label','#currentmodule'))</label>
      <animation effect="slide" start="-771,0" end="0,0" tween="quadratic" time="300" delay="0">WindowOpen</animation>
    </control>
  </controls>
</window>
