<window>
  <id>6803</id>
  <defaultcontrol>50</defaultcontrol>
  <allowoverlay>yes</allowoverlay>
  <define>#header.label:SBS Gemist</define>
  <define>#header.hover:hover_tvgemist.png</define>
  <define>#currentmodule:TV Gemist</define>
  <define>#extended:SBS Gemist</define>
  <controls>
    <import>tvgemist.window.xml</import>
    <control>
      <type>image</type>
      <posX>200</posX>
      <posY>150</posY>
      <description>SBS Gemist image</description>
      <id>123</id>
      <texture>i</texture>
    </control>
    <control>
      <type>image</type>
      <id>1</id>
      <posX>0</posX>
      <posY>50</posY>
      <width>256</width>
      <height>720</height>
      <texture>homebladesub.png</texture>
      <animation effect="slide" time="10" end="-300,0">WindowClose</animation>
      <animation effect="slide" time="10" start="-300,0">WindowOpen</animation>
    </control>
    <control>
      <type>group</type>
      <description>group element</description>
      <animation effect="slide" time="10" end="-300,0">WindowClose</animation>
      <animation effect="slide" time="10" start="-300,0">WindowOpen</animation>
      <posX>20</posX>
      <posY>300</posY>
      <layout>StackLayout</layout>
      <control>
        <description>Favorieten</description>
        <type>button</type>
        <id>2</id>
        <label>Favorieten</label>
        <onup>17</onup>
        <ondown>3</ondown>
        <font>mediastream12tc</font>
      </control>
      <control>
        <description>Series</description>
        <type>button</type>
        <id>3</id>
        <label>Series</label>
        <onup>2</onup>
        <ondown>50</ondown>
        <font>mediastream12tc</font>
      </control>
    </control>
    <import>common.facade.music.xml</import>
    <import>common.time.xml</import>
  </controls>
</window>
