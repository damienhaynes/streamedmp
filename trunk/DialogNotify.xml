<?xml version="1.0" encoding="UTF-8"?>
<window>
  <id>2016</id>
  <defaultcontrol>4</defaultcontrol>
  <allowoverlay>no</allowoverlay>
  <controls>
    
    <control>
      <type>image</type>
      <id>0</id>
      <width>1280</width>
      <height>720</height>
      <texture>black.png</texture>
      <colordiffuse>90ffffff</colordiffuse>
      <animation effect="fade" time="200">windowopen</animation>
      <animation effect="fade" time="200">windowclose</animation>
    </control>
    
    <control>
      <type>image</type>
      <id>0</id>
      <posX>375</posX>
      <posY>228</posY>
      <width>532</width>
      <height>23</height>
      <texture>dialogtop.png</texture>
     
    </control>
    <control>
      <type>image</type>
      <id>0</id>
      <posX>375</posX>
      <posY>251</posY>
      <width>532</width>
      <height>180</height>
      <texture>dialogmiddle.png</texture>
         </control>
    <control>
      <type>image</type>
      <id>0</id>
      <posX>375</posX>
      <posY>430</posY>
      <width>532</width>
      <height>23</height>
      <texture>dialogbottom.png</texture>
    </control>
    

        
      <control>
        <description>Heading text label</description>
        <type>label</type>
        <id>3</id>
        <posX>360</posX>
	<posY>245</posY>
	<width>560</width>
        <label>181</label>
	<font>mediastream11tc</font>
        <align>center</align>
      </control>

    
      <control>
        <description>logo</description>
        <type>image</type>
        <id>5</id>
        <posX>400</posX>
        <posY>300</posY>
        <width>75</width>
        <height>75</height>
        <texture>#thumb</texture>
        <colorkey>FFFF00FF</colorkey>
        <colordiffuse>FFFFFFFF</colordiffuse>
        <keepaspectratio>yes</keepaspectratio>
      </control>

      <control>
        <description>textarea</description>
        <type>textbox</type>
        <id>6</id>
        <font>mediastream12</font>
        <posX>525</posX>
        <posY>300</posY>
        <width>350</width>
        <height>190</height>
        <spinPosX>873</spinPosX>
        <spinPosY>455</spinPosY>
        <onleft>4</onleft>
        <onright>4</onright>
        <onup>4</onup>
        <ondown>4</ondown>
        <label>-</label>
      </control>
    
      <control>
        <description>OK button</description>
        <type>button</type>
        <id>10</id>
        <font>mediastream11tc</font>
        <textureFocus>dialogbigbuttonfo.png</textureFocus>
        <textureNoFocus>dialogbigbuttonnf.png</textureNoFocus>
        <width>128</width>
        <posX>574</posX>
        <posY>390</posY>
        <textXOff>1</textXOff>
        <textYOff>1</textYOff>
        <textalign>center</textalign>
        <label>Cancel</label>
        <onleft>10</onleft>
        <onright>10</onright>
        <onup>10</onup>
        <ondown>10</ondown>
      </control>
    
  </controls>
</window>
