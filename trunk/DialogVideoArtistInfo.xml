      <id>0</id>
      <posX>309</posX>
      <posY>282</posY>
      <width>900</width>
      <font>mediastream11tc</font>
      <textcolor>ff909090</textcolor>
			<shadowAngle>45</shadowAngle>
			<shadowDistance>3</shadowDistance>
			<shadowColor>#shadow.dark</shadowColor>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="250">WindowClose</animation>
    </control>    
    <control>
      <description>Biography</description>
      <type>textboxscrollup</type>
      <id>20</id>
      <posX>308</posX>
      <posY>315</posY>
      <width>510</width>
      <height>320</height>
      <textcolor>ffFFFFFF</textcolor>
      <seperator>-----------------------------------------------------------------------------------</seperator>
      <font>mediastream11</font>
      <label>#Actor.Biography</label>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="250">WindowClose</animation>
    </control>
    <control>
      <description>Movies Starred In</description>
      <type>textbox</type>
      <id>22</id>
			<onleft>3</onleft>
      <posX>308</posX>
      <posY>315</posY>
      <width>430</width>
      <height>340</height>
      <textcolor>ffFFFFFF</textcolor>
      <seperator>-----------------------------------------------------------------------------------</seperator>
      <font>mediastream11</font>
      <label>#Actor.Movies</label>	 
			<spinWidth>25</spinWidth>
			<spinHeight>25</spinHeight>
			<spinPosX>680</spinPosX>
			<spinPosY>620</spinPosY>
			<spinAlign>Right</spinAlign>
			<spinColor>ff006698</spinColor>
			<textureUp>arrow_round_up_nofocus.png</textureUp>
			<textureDown>arrow_round_down_nofocus.png</textureDown>
			<textureUpFocus>arrow_round_up_focus.png</textureUpFocus>
			<textureDownFocus>arrow_round_down_focus.png</textureDownFocus>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="250">WindowClose</animation>
    </control>
