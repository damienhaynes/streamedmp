<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<window>
  <allowoverlay>yes</allowoverlay>
  <controls>
    <!-- *** Starry Sky *** -->
    <control>
      <description>TV Frame</description>
      <type>image</type>
      <id>0</id>
      <posX>435</posX>
      <posY>60</posY>
      <width>1035</width>
      <height>861</height>
      <texture>LCD.png</texture>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="250">WindowClose</animation>
      <visible>string.equals(#WorldWeather.View,Astronomy)</visible>
    </control>
    <control>
      <description>Starry Sky label</description>
      <type>label</type>
      <id>0</id>
      <posX>960</posX>
      <posY>889</posY>
      <label>#WorldWeather.TranslationStarrySky</label>
      <font>mediastream16tc</font>
      <align>center</align>
      <textcolor>white</textcolor>
      <visible>string.equals(#WorldWeather.View,Astronomy)</visible>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="250">WindowClose</animation>
    </control>
    <control>
      <description>Starry Sky image</description>
      <type>image</type>
      <id>0</id>
      <posX>577</posX>
      <posY>129</posY>
      <width>741</width>
      <height>718</height>
      <texture>#WorldWeather.ImageStarrySky</texture>
      <visible>string.equals(#WorldWeather.View,Astronomy)</visible>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="250">WindowClose</animation>
    </control>
  </controls>
</window>
