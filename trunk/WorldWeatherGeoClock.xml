<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<!--version="1.0" encoding="utf-8" standalone="yes"-->
<window>
  <allowoverlay>yes</allowoverlay>
  <controls>
    <!-- *** GeoClock *** -->
    <control>
      <description>TV Frame</description>
      <type>image</type>
      <id>0</id>
      <posX>210</posX>
      <posY>60</posY>
      <width>1500</width>
      <height>861</height>
      <texture>LCD.png</texture>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="250">WindowClose</animation>
      <visible>string.equals(#WorldWeather.View,GeoClock)</visible>
    </control>
    <control>
      <description>Location label</description>
      <type>label</type>
      <id>0</id>
      <posX>960</posX>
      <posY>889</posY>
      <label>#WorldWeather.TranslationGeoClock</label>
      <font>mediastream16tc</font>
      <align>center</align>
      <textcolor>white</textcolor>
      <visible>string.equals(#WorldWeather.View,GeoClock)</visible>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="250">WindowClose</animation>
    </control>
    <control>
      <description>GeoClock image</description>
      <type>image</type>
      <id>0</id>
      <posX>279</posX>
      <posY>129</posY>
      <width>1359</width>
      <height>720</height>
      <texture>#WorldWeather.ImageGeoClock</texture>
      <visible>string.equals(#WorldWeather.View,GeoClock)</visible>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="250">WindowClose</animation>
    </control>
  </controls>
</window>
