<?xml version="1.0" encoding="utf-8"?>
<window>
  <controls>
    <control>
      <description>Selected item Background</description>
      <type>image</type>
      <id>0</id>
      <posX>0</posX>
      <posY>675</posY>
      <width>1280</width>
      <height>47</height>
      <texture>imagestreamtitle.png</texture>
      <animation effect="fade" time="300" delay="0">WindowOpen</animation>
      <animation effect="fade" time="230" delay="0">WindowClose</animation>
    </control>
    <control>
      <description>Current SMS T9 Filter</description>
      <type>fadelabel</type>
      <id>1</id>
      <posX>25</posX>
      <posY>684</posY>
      <width>950</width>
      <font>mediastream16tc</font>
      <label>#OnlineVideos.filter</label>
      <visible>string.equals(#selectedindex,)+!string.equals(#OnlineVideos.IsBuffering, true)</visible>
      <animation effect="fade" time="300" delay="0">WindowOpen</animation>
      <animation effect="fade" time="230" delay="0">WindowClose</animation>
    </control>
    <control>
      <description>Selected item Label</description>
      <type>fadelabel</type>
      <id>1</id>
      <posX>25</posX>
      <posY>684</posY>
      <width>950</width>
      <font>mediastream16tc</font>
      <label>#selecteditem</label>
      <visible>!string.equals(#selectedindex,)+!string.equals(#OnlineVideos.state, details)+!string.equals(#OnlineVideos.IsBuffering, true)</visible>
      <animation effect="fade" time="300" delay="0">WindowOpen</animation>
      <animation effect="fade" time="230" delay="0">WindowClose</animation>
    </control>
    <control>
      <description>Buffer Progress Bar</description>
      <type>tvprogress</type>
      <id>1</id>
      <posX>0</posX>
      <posY>681</posY>
      <width>950</width>
      <height>47</height>
      <toptexture>-</toptexture>
      <TextureOffsetY>21</TextureOffsetY>
      <bottomtexture>-</bottomtexture>
      <texturetick>-</texturetick>
      <lefttexture>-</lefttexture>
      <midtexture>-</midtexture>
      <righttexture>-</righttexture>
      <logotexture>-</logotexture>
      <fillbackgroundtexture>-</fillbackgroundtexture>
      <fillbgxoff>-3</fillbgxoff>
      <fillbgyoff>0</fillbgyoff>
      <filltexture1>osdprogressmid_blue.png</filltexture1>
      <filltexture2>-</filltexture2>
      <filltexture3>-</filltexture3>
      <fillheight>34</fillheight>
      <label>#OnlineVideos.buffered</label>
      <label1>-</label1>
      <label2>-</label2>
      <font>mediastream10c</font>
      <textcolor>ff888888</textcolor>
      <visible>string.equals(#OnlineVideos.IsBuffering, true) + !player.hasmedia</visible>
    </control>
    <control>
      <description>Buffering Label</description>
      <type>fadelabel</type>
      <id>1</id>
      <posX>25</posX>
      <posY>684</posY>
      <width>950</width>
      <font>mediastream16tc</font>
      <shadowAngle>45</shadowAngle>
      <shadowDistance>3</shadowDistance>
      <shadowColor>ff111111</shadowColor>
      <label>#OnlineVideos.buffered % #OnlineVideos.Translation.Buffered.Label</label>
      <visible>string.equals(#OnlineVideos.IsBuffering, true) + !player.hasmedia</visible>
      <animation effect="fade" time="300" delay="0">WindowOpen</animation>
      <animation effect="fade" time="230" delay="0">WindowClose</animation>
    </control>
    <control>
      <description>Page Count Background</description>
      <type>image</type>
      <id>0</id>
      <posX>1010</posX>
      <posY>683</posY>
      <width>260</width>
      <height>32</height>
      <texture>mediacountbg_Spin.png</texture>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="250">WindowClose</animation>
      <animation effect="slide" time="150" start="800,0">WindowOpen</animation>
      <animation effect="slide" time="250" end="800,0">WindowClose</animation>
    </control>
    <control>
      <description>Page Count Label</description>
      <type>label</type>
      <label>30009</label>
      <id>0</id>
      <posX>1020</posX>
      <posY>688</posY>
      <font>mediastream12tc</font>
      <textcolor>ff909090</textcolor>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="250">WindowClose</animation>
      <animation effect="slide" time="150" start="800,0">WindowOpen</animation>
      <animation effect="slide" time="250" end="800,0">WindowClose</animation>
    </control>
    <control>
      <description>Item Count Background</description>
      <type>image</type>
      <id>0</id>
      <posX>1065</posX>
      <posY>13</posY>
      <width>202</width>
      <height>28</height>
      <texture>mediacountbg.png</texture>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="250">WindowClose</animation>
      <visible>!string.equals(#OnlineVideos.state, details)</visible>
    </control>
    <control>
      <description>Items Translation</description>
      <type>label</type>
      <label>507</label>
      <id>0</id>
      <width>90</width>
      <posX>1077</posX>
      <posY>17</posY>
      <align>left</align>
      <font>mediastream10tc</font>
      <textcolor>ff909090</textcolor>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="250">WindowClose</animation>
      <visible>!string.equals(#OnlineVideos.state, details)</visible>
    </control>
    <control>
      <description>Item Count</description>
      <type>label</type>
      <label>#itemcount</label>
      <id>0</id>
      <posX>1240</posX>
      <posY>16</posY>
      <font>mediastream10c</font>
      <textcolor>ff006698</textcolor>
      <align>right</align>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="250">WindowClose</animation>
      <visible>!string.equals(#OnlineVideos.state, details)</visible>
    </control>
    <control>
      <description>group element</description>
      <type>group</type>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="250">WindowClose</animation>
      <animation effect="slide" time="250" start="800,0">WindowOpen</animation>
      <animation effect="slide" time="250" end="800,0">WindowClose</animation>
      <!-- *** CLIPS LIST *** -->
      <control>
        <description>composite control consisting of a list control and a thumbnail panel</description>
        <type>facadeview</type>
        <id>50</id>
        <control>
          <description>List</description>
          <type>listcontrol</type>
          <id>50</id>
          <posX>690</posX>
          <posY>240</posY>
          <height>480</height>
          <width>660</width>
          <onleft>2</onleft>
          <onright>2</onright>
          <scrollOffset>4</scrollOffset>
          <itemWidth>42</itemWidth>
          <itemHeight>32</itemHeight>
          <IconXOff>-30</IconXOff>
          <IconYOff>7</IconYOff>
          <font2>mediastream10</font2>
          <keepaspectratio>yes</keepaspectratio>
          <spinPosX>1172</spinPosX>
          <spinPosY>681</spinPosY>
          <spinAlign>Right</spinAlign>
          <textureUp>arrow_round_up_nofocus.png</textureUp>
          <textureDown>arrow_round_down_nofocus.png</textureDown>
          <textureUpFocus>arrow_round_up_focus.png</textureUpFocus>
          <textureDownFocus>arrow_round_down_focus.png</textureDownFocus>
          <spinWidth>50</spinWidth>
          <spinHeight>37</spinHeight>
          <suffix>|</suffix>
        </control>
        <control>
          <description>Thumbnail Panel</description>
          <type>thumbnailpanel</type>
          <id>50</id>
          <posX>30</posX>
          <posY>240</posY>
          <width>1270</width>
          <height>475</height>
          <scrollOffset>0</scrollOffset>
          <onleft>2</onleft>
          <onright>2</onright>
          <frameNoFocus>imagebg.png</frameNoFocus>
          <frameFocus>thumbborder5.png</frameFocus>
          <!-- Small Icons -->
          <itemWidth>135</itemWidth>
          <itemHeight>110</itemHeight>
          <textureWidth>131</textureWidth>
          <textureHeight>100</textureHeight>
          <thumbWidth>121</thumbWidth>
          <thumbHeight>92</thumbHeight>
          <thumbPosX>5</thumbPosX>
          <thumbPosY>4</thumbPosY>
          <!-- Large Icons -->
          <itemWidthBig>243</itemWidthBig>
          <itemHeightBig>200</itemHeightBig>
          <textureWidthBig>240</textureWidthBig>
          <textureHeightBig>180</textureHeightBig>
          <thumbWidthBig>221</thumbWidthBig>
          <thumbHeightBig>166</thumbHeightBig>
          <thumbPosXBig>9</thumbPosXBig>
          <thumbPosYBig>7</thumbPosYBig>
          <!-- Page Spin Buttons -->
          <textureUp>arrow_round_up_nofocus.png</textureUp>
          <textureDown>arrow_round_down_nofocus.png</textureDown>
          <textureUpFocus>arrow_round_up_focus.png</textureUpFocus>
          <textureDownFocus>arrow_round_down_focus.png</textureDownFocus>
          <spinPosX>1172</spinPosX>
          <spinPosY>681</spinPosY>
          <spinWidth>50</spinWidth>
          <spinHeight>37</spinHeight>
          <spinAlign>Right</spinAlign>
          <spinColor>ff006698</spinColor>
          <spinCanFocus>no</spinCanFocus>
        </control>
        <control>
          <description>playlist listcontrol</description>
          <type>playlistcontrol</type>
          <id>50</id>
          <scrollOffset>2</scrollOffset>
          <spinPosX>1172</spinPosX>
          <spinPosY>681</spinPosY>
          <textureUp>arrow_round_up_nofocus.png</textureUp>
          <textureDown>arrow_round_down_nofocus.png</textureDown>
          <textureUpFocus>arrow_round_up_focus.png</textureUpFocus>
          <textureDownFocus>arrow_round_down_focus.png</textureDownFocus>
          <spinWidth>50</spinWidth>
          <spinHeight>37</spinHeight>
          <spinAlign>Right</spinAlign>
          <spinColor>ff006698</spinColor>
          <spinCanFocus>yes</spinCanFocus>
          <onleft>2</onleft>
          <onright>2</onright>
        </control>
      </control>
    </control>
  </controls>
</window>
