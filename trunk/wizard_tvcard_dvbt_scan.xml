      <label>200009</label>
    </control>
    <control>
      <type>button</type>
      <description>Back</description>
      <id>25</id>
      <onleft>24</onleft>
      <onright>24</onright>
      <onup>5</onup>
      <ondown>24</ondown>
      <hyperlink>617</hyperlink>
      <label>200007</label>
    </control>
    </control>
    <control>
      <type>label</type>
      <description>-</description>
