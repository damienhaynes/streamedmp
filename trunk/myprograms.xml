        <control>
          <description>View-As</description>
          <type>button</type>
          <id>2</id>
          <label>100</label>
          <onright>50</onright>
          <onup>17</onup>
        </control>
        <control>
          <description>Switch View</description>
          <type>button</type>
          <id>4</id>
          <label>457</label>
          <onright>50</onright>
          <ondown>3</ondown>
        </control>
        <control>
          <description>Refresh</description>
          <type>button</type>
          <id>3</id>
          <label>184</label>
          <onright>50</onright>
          <ondown>99</ondown>
        </control>
      </control>
    </control>
    <import>common.facade.xml</import>
  </controls>
