<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<window>
  <controls>
    <control>
      <description>group element</description>
      <type>group</type>
      <visible>!Control.HasFocus(50)</visible>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="250">WindowClose</animation>
      <animation effect="slide" time="150" start="0,-300">WindowOpen</animation>
      <animation effect="slide" time="250" end="800,0">WindowClose</animation>
      <animation effect="rotatey" start="15" end="0" center="600,0" tween="back" ease="easeout" time="500" reversible="false">hidden</animation>
      <animation effect="rotatey" start="0" end="15" center="600,0" tween="back" ease="easeout" time="500" reversible="false">visible</animation>
      <control>
        <description>composite control consisting of a list control and a thumbnail panel</description>
        <type>facadeview</type>
        <id>50</id>
        <control>
          <description>listcontrol</description>
          <type>listcontrol</type>
          <id>50</id>
          <posY>250</posY>
          <onleft>2</onleft>
          <onright>2</onright>
          <height>450</height>
          <textureHeight>55</textureHeight>
          <scrollOffset>2</scrollOffset>
          <font>mediastream12tcb</font>
          <font2>mediastream12tcb</font2>
          <IconXOff>5</IconXOff>
          <IconYOff>5</IconYOff>
          <textXOff>25</textXOff>
          <itemWidth>45</itemWidth>
          <itemHeight>45</itemHeight>
          <unfocusedAlpha>100</unfocusedAlpha>
        </control>
        <control>
          <description>album view listcontrol</description>
          <type>listcontrol</type>
          <subtype>album</subtype>
          <id>50</id>
          <font>font13</font>
          <font2>font13</font2>
          <selectedColor>80000000</selectedColor>
          <textcolor>ffffffff</textcolor>
          <selectedColor2>ffffffff</selectedColor2>
          <textcolor2>ffffffff</textcolor2>
          <textXOff2>420</textXOff2>
          <textYOff2>60</textYOff2>
          <itemWidth>72</itemWidth>
          <itemHeight>72</itemHeight>
          <spaceBetweenItems>14</spaceBetweenItems>
          <textureHeight>90</textureHeight>
          <scrollOffset>1</scrollOffset>
          <textureFocus>album_background_focus.png</textureFocus>
          <textureNoFocus>album_background_nofocus.png</textureNoFocus>
          <onleft>2</onleft>
          <onright>50</onright>
          <onup>2</onup>
          <ondown>2</ondown>
        </control>
        <control>
          <description>Thumbnail Panel</description>
          <type>thumbnailpanel</type>
          <id>50</id>
          <posX>50</posX>
          <posY>105</posY>
          <width>660</width>
          <height>471</height>
          <scrollOffset>0</scrollOffset>
          <onleft>2</onleft>
          <onright>2</onright>
          <textureWidthBig>96</textureWidthBig>
          <textureHeightBig>96</textureHeightBig>
          <itemWidthBig>130</itemWidthBig>
          <thumbWidthBig>100</thumbWidthBig>
          <thumbWidthBig>82</thumbWidthBig>
          <thumbHeightBig>82</thumbHeightBig>
          <thumbPosXBig>6</thumbPosXBig>
          <thumbPosYBig>6</thumbPosYBig>
          <thumbZoom>no</thumbZoom>
          <enableFocusZoom>no</enableFocusZoom>
          <zoomXPixels>30</zoomXPixels>
          <zoomYPixels>30</zoomYPixels>
          <hideUnfocusTexture>no</hideUnfocusTexture>
          <renderFocusText>no</renderFocusText>
          <renderUnfocusText>yes</renderUnfocusText>
        </control>
        <control>
          <description>Filmstrip view</description>
          <type>filmstrip</type>
          <dimColor>90ffffff</dimColor>
          <id>50</id>
          <scrollOffset>1</scrollOffset>
          <posX>0</posX>
          <posY>220</posY>
          <width>700</width>
          <height>576</height>
          <onup>2</onup>
          <onleft>2</onleft>
          <thumbWidth>143</thumbWidth>
          <thumbHeight>143</thumbHeight>
          <thumbPosX>4</thumbPosX>
          <thumbPosY>4</thumbPosY>
          <itemWidth>158</itemWidth>
          <itemHeight>185</itemHeight>
          <textureWidth>152</textureWidth>
          <textureHeight>152</textureHeight>
          <imageFolderFocus>Picture_cover.png</imageFolderFocus>
          <frame>Picture_cover.png</frame>
          <frameFocus>Picture_cover.png</frameFocus>
          <background>Coverbig.png</background>
          <thumbs flipY="true" diffuse="Thumb_Mask.png"></thumbs>
          <InfoImage flipY="true" diffuse="Thumb_Mask.png"></InfoImage>
          <showFrame>yes</showFrame>
          <showFolder>yes</showFolder>
          <showBackGround>no</showBackGround>
          <showInfoImage>no</showInfoImage>
          <thumbAnimation effect="zoom" start="100,100" end="130,130" time="200">focus</thumbAnimation>
          <thumbAnimation effect="zoom" start="130,130" end="100,100" time="200">unfocus</thumbAnimation>
          <animation effect="rotatey" start="20" end="0" center="525,0" tween="back" ease="easeout" time="500" reversible="false">unfocus</animation>
          <animation effect="rotatey" start="0" end="20" center="525,0" tween="back" ease="easeout" time="500" reversible="false">focus</animation>
        </control>
        <control>
          <description>playlist listcontrol</description>
          <type>playlistcontrol</type>
          <id>50</id>
          <onleft>2</onleft>
          <onright>2</onright>
          <scrollOffset>2</scrollOffset>
        </control>
      </control>
    </control>
  </controls>
</window>