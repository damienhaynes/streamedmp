<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<window>
  <id>5000</id>
  <defaultcontrol>3</defaultcontrol>
  <allowoverlay>yes</allowoverlay>
  <define>#header.label:850</define>
  <define>#header.image:-</define>
  <define>#header.hover:hover_my alarm.png</define>
  <controls>
    <import>common.window.xml</import>
    <import>common.time.xml</import>

    <control>
      <type>group</type>
      <description>group element</description>
      <animation effect="slide" time="10" end="-300,0">WindowClose</animation>
      <animation effect="slide" time="10" start="-300,0">WindowOpen</animation>
      <posX>20</posX>
      <posY>80</posY>
      <layout>StackLayout</layout>
      <control>
        <description>New button</description>
        <type>button</type>
        <id>3</id>
        <label>617</label>
        <onright>50</onright>
        <onleft>50</onleft>
        <onup>17</onup>
        <ondown>4</ondown>
      </control>
      <control>
        <description>Sleep timer Button</description>
        <type>button</type>
        <id>4</id>
        <label>859</label>
        <onright>5</onright>
        <onleft>5</onleft>
        <onup>3</onup>
        <ondown>3</ondown>
      </control>
    </control>
    <import>common.facade.xml</import>
  </controls>
</window>