<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<window>
  <id>7890</id>
  <defaultcontrol>10</defaultcontrol>
  <allowoverlay>no</allowoverlay>
  <define>#header.hover:hover_my music.png</define>
  <define>#useRandomMusicFanart:Yes</define>
  <define>#usePlayFanart:Yes</define>
  <controls>
    <import>mymusic.autofanart.xml</import>
    <control>
      <id>0</id>
      <type>image</type>
      <posX>0</posX>
      <posY>450</posY>
      <width>1275</width>
      <height>290</height>
      <texture>last.fm.background.png</texture>
      <colordiffuse>FAFFFFFF</colordiffuse>
      <animation effect="Fade" time="400">WindowOpen</animation>
      <animation effect="Fade" time="300">WindowClose</animation>
    </control>
    <!--            :: MENU ::            -->
    <control>
      <type>group</type>
      <description>group element</description>
      <posX>1006</posX>
      <posY>485</posY>
      <width>480</width>
      <height>720</height>
      <layout>StackLayout</layout>
      <control>
        <description>start a stream</description>
        <type>button</type>
        <id>10</id>
        <label>34001</label>
        <onup>1142</onup>
        <onleft>123</onleft>
        <textureFocus>listbg_fo.png</textureFocus>
        <textureNoFocus>listbg_nf.png</textureNoFocus>
      </control>
      <control>
        <description>choose artist</description>
        <type>button</type>
        <id>15</id>
        <label>Artists</label>
        <onleft>123</onleft>
        <textureFocus>listbg_fo.png</textureFocus>
        <textureNoFocus>listbg_nf.png</textureNoFocus>
      </control>
      <control>
        <description>chose tag</description>
        <type>button</type>
        <id>20</id>
        <label>Tags</label>
        <onleft>123</onleft>
        <textureFocus>listbg_fo.png</textureFocus>
        <textureNoFocus>listbg_nf.png</textureNoFocus>
      </control>
      <control>
        <description>chose friend</description>
        <type>button</type>
        <id>30</id>
        <label>Friends</label>
        <onleft>123</onleft>
        <textureFocus>listbg_fo.png</textureFocus>
        <textureNoFocus>listbg_nf.png</textureNoFocus>
      </control>
      <control>
        <description>toggle submit mode</description>
        <type>checkbutton</type>
        <id>35</id>
        <label>34014</label>
        <onleft>123</onleft>
        <textureFocus>listbg_fo.png</textureFocus>
        <textureNoFocus>listbg_nf.png</textureNoFocus>
        <visible>true</visible>
      </control>
      <control>
        <description>toggle discovery mode</description>
        <type>checkbutton</type>
        <id>40</id>
        <label>34013</label>
        <onleft>123</onleft>
        <textureFocus>listbg_fo.png</textureFocus>
        <textureNoFocus>listbg_nf.png</textureNoFocus>
        <visible>false</visible>
      </control>
      <control>
        <description>My Lyrics</description>
        <type>button</type>
        <id>1142</id>
        <label>Lyrics</label>
        <onleft>123</onleft>
        <ondown>1143</ondown>
        <hyperlink>90478</hyperlink>
        <visible>plugin.isenabled(My Lyrics)</visible>
        <textureFocus>listbg_fo.png</textureFocus>
        <textureNoFocus>listbg_nf.png</textureNoFocus>
      </control>
      <control>
        <description>Similar Tags</description>
        <type>button</type>
        <id>1143</id>
        <label>Show Similar tags</label>
        <onleft>1143</onleft>
        <ondown>10</ondown>
        <textureFocus>listbg_fo.png</textureFocus>
        <textureNoFocus>listbg_nf.png</textureNoFocus>
      </control>
    </control>
    <!-- Artist Image -->
    <control>
      <type>image</type>
      <description>artist art image</description>
      <id>112</id>
      <posX>27</posX>
      <posY>561</posY>
      <width>140</width>
      <height>140</height>
      <keepaspectratio>no</keepaspectratio>
      <texture>#Play.Current.ArtistThumb</texture>
      <visible>Player.HasAudio</visible>
    </control>
    <control>
      <id>0</id>
      <description>album art overlay</description>
      <type>image</type>
      <posX>22</posX>
      <posY>556</posY>
      <width>150</width>
      <height>150</height>
      <texture>mayacdborder.png</texture>
      <animation effect="Fade" time="800">WindowOpen</animation>
      <animation effect="Fade" time="300">WindowClose</animation>
    </control>
    <control>
      <type>fadelabel</type>
      <description>ARTIST label</description>
      <id>0</id>
      <posX>25</posX>
      <posY>485</posY>
      <width>700</width>
      <height>26</height>
      <label>#Play.Current.Artist</label>
      <font>mediastream14tc</font>
    </control>
    <control>
      <type>fadelabel</type>
      <description>TRACK TITLE label</description>
      <id>0</id>
      <posX>25</posX>
      <posY>510</posY>
      <width>660</width>
      <height>26</height>
      <label>#Play.Current.Title</label>
      <font>mediastream12tc</font>
    </control>
    <control>
      <type>fadelabel</type>
      <description>ALBUM label</description>
      <id>0</id>
      <posX>25</posX>
      <posY>530</posY>
      <width>660</width>
      <height>26</height>
      <label>#Play.Current.Album</label>
      <font>mediastream12c</font>
    </control>
    <control>
      <type>label</type>
      <description>CURRENT STREAM label</description>
      <id>0</id>
      <posX>975</posX>
      <posY>485</posY>
      <width>400</width>
      <height>26</height>
      <align>right</align>
      <label>#Play.Current.Lastfm.CurrentStream</label>
      <font>mediastream11</font>
    </control>
    <control>
      <type>label</type>
      <description>PLAYTIME DURATION label</description>
      <id>0</id>
      <posX>975</posX>
      <posY>685</posY>
      <width>160</width>
      <height>26</height>
      <align>right</align>
      <label>#currentplaytime / #trackduration</label>
      <visible>Player.HasAudio</visible>
      <font>mediastream12tc</font>
    </control>
    <!-- Similar Artist and Tags -->
    <control>
      <type>label</type>
      <description>similar hint</description>
      <id>443</id>
      <posX>230</posX>
      <posY>555</posY>
      <label>34039</label>
      <font>mediastream10tc</font>
      <align>left</align>
      <textcolor>ff006698</textcolor>
      <visible>control.hastext(444)+control.hasfocus(1143)</visible>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="250">WindowClose</animation>
    </control>
    <control>
      <type>textbox</type>
      <description>Similar artists box</description>
      <id>444</id>
      <posX>230</posX>
      <posY>570</posY>
      <width>800</width>
      <font>mediastream12</font>
      <label>#Play.Current.Lastfm.SimilarArtists</label>
      <visible>control.hasfocus(1143)</visible>
    </control>
    <control>
      <type>label</type>
      <description>tag hint</description>
      <id>332</id>
      <posX>230</posX>
      <posY>620</posY>
      <label>34041</label>
      <font>mediastream10tc</font>
      <align>left</align>
      <textcolor>ff006698</textcolor>
      <visible>control.hastext(333)+control.hasfocus(1143)</visible>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="250">WindowClose</animation>
    </control>
    <control>
      <type>textbox</type>
      <description>Track tags box</description>
      <id>333</id>
      <posX>230</posX>
      <posY>635</posY>
      <width>800</width>
      <font>mediastream11</font>
      <label>#Play.Current.Lastfm.TrackTags</label>
      <visible>control.hasfocus(1143)</visible>
    </control>
    <control>
      <type>listcontrol</type>
      <description>current playlist control</description>
      <id>123</id>
      <posX>200</posX>
      <posY>552</posY>
      <height>356</height>
      <width>600</width>
      <onright>10</onright>
      <onleft>10</onleft>
      <font>mediastream11c</font>
      <font2>mediastream11c</font2>
      <font3>mediastream11c</font3>
      <!--<textcolor2>80ffffff</textcolor2>-->
      <textXOff>5</textXOff>
      <textXOff2>75</textXOff2>
      <textXOff3>222</textXOff3>
      <textYOff>5</textYOff>
      <textYOff2>22</textYOff2>
      <textYOff3>22</textYOff3>
      <IconXOff>0</IconXOff>
      <IconYOff>5</IconYOff>
      <onright>10</onright>
      <textureFocus>listbg_fo.png</textureFocus>
      <textureNoFocus>listbg_nf.png</textureNoFocus>
      <keepaspectratio>no</keepaspectratio>
      <textureHeight>30</textureHeight>
      <itemHeight>25</itemHeight>
      <itemWidth>25</itemWidth>
      <spaceBetweenItems>0</spaceBetweenItems>
      <visible allowhiddenfocus="True">player.hasmedia+!control.hasfocus(1143)</visible>
    </control>
    <import>common.time.xml</import>
  </controls>
</window>
