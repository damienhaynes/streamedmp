<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<window>
  <id>500</id>
  <defaultcontrol>39</defaultcontrol>
  <allowoverlay>yes</allowoverlay>
  <define>#header.label:136</define>
  <define>#header.image:-</define>
  <define>#header.hover:hover_musicvideo.png</define>
  <controls>
    <import>common.window.xml</import>
    <import>common.time.xml</import>

    <control>
      <type>group</type>
      <description>group element</description>
      <animation effect="slide" time="10" end="-300,0">WindowClose</animation>
      <animation effect="slide" time="10" start="-300,0">WindowOpen</animation>
      <posX>20</posX>
      <posY>80</posY>
      <layout>StackLayout</layout>
      <control>
        <description>my playlist button</description>
        <type>button</type>
        <id>39</id>
        <label>983</label>
        <onright>50</onright>
      </control>
      <control>
        <description>Shuffle button</description>
        <type>button</type>
        <id>20</id>
        <label>191</label>
        <onright>50</onright>
      </control>
      <control>
        <description>Save button</description>
        <type>button</type>
        <id>21</id>
        <label>190</label>
        <onright>50</onright>
      </control>
      <control>
        <description>Clear button</description>
        <type>button</type>
        <id>22</id>
        <label>192</label>
        <onright>50</onright>
      </control>
      <control>
        <description>Play button</description>
        <type>button</type>
        <id>23</id>
        <label>208</label>
        <onright>50</onright>
      </control>
      <control>
        <description>Next button</description>
        <type>button</type>
        <id>24</id>
        <label>209</label>
        <onright>50</onright>
      </control>
      <control>
        <description>Previous button</description>
        <type>button</type>
        <id>25</id>
        <label>210</label>
        <onright>50</onright>
        <ondown>99</ondown>
      </control>
    </control>
    <control>
      <description>group element</description>
      <type>group</type>
      <animation effect="slide" time="10" start="0,-300">WindowOpen</animation>
      <animation effect="slide" time="10" end="800,0">WindowClose</animation>
      <control>
        <description>composite control consisting of a list control and a thumbnail panel</description>
        <type>facadeview</type>
        <id>50</id>
        <control>
          <description>Thumbnail Panel</description>
          <type>thumbnailpanel</type>
          <id>50</id>
          <onleft>20</onleft>
          <onright>50</onright>
          <onup>20</onup>
          <ondown>20</ondown>
        </control>
        <control>
          <description>album view listcontrol</description>
          <type>listcontrol</type>
          <subtype>album</subtype>
          <id>50</id>
          <font>font12</font>
          <font2>font12</font2>
          <selectedColor>FFFF6600</selectedColor>
          <textcolor>ffffffff</textcolor>
          <selectedColor2>FFFF6600</selectedColor2>
          <textcolor2>ffffffff</textcolor2>
          <textXOff>15</textXOff>
          <textYOff>15</textYOff>
          <textXOff2>1030</textXOff2>
          <textYOff2>45</textYOff2>
          <itemWidth>72</itemWidth>
          <itemHeight>72</itemHeight>
          <spaceBetweenItems>0</spaceBetweenItems>
          <textureHeight>96</textureHeight>
          <scrollOffset>1</scrollOffset>
          <textureFocus>album_background_focus.png</textureFocus>
          <textureNoFocus>album_background_nofocus.png</textureNoFocus>
          <spinPosY>560</spinPosY>
          <onleft>20</onleft>
          <onright>50</onright>
          <onup>20</onup>
          <ondown>20</ondown>
        </control>
        <control>
          <description>playlist control</description>
          <type>playlistcontrol</type>
          <id>50</id>
        </control>
      </control>
    </control>
  </controls>
</window>