      <font>mediastream11</font>
      <label>#AlbumInfo.Review</label>
      <scrollStartDelaySec>15</scrollStartDelaySec>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="250">WindowClose</animation>
      <visible>!skin.hassetting(#AlbumInfo.ShowTracks)</visible>
    </control>
    <control>
      <description>Tracks</description>
      <type>textboxscrollup</type>
      <id>1</id>
      <posX>462</posX>
      <posY>435</posY>
      <width>1425</width>
      <height>322</height>
      <textcolor>ffFFFFFF</textcolor>
      <seperator>-----------------------------------------------------------------------------------</seperator>
      <font>mediastream11</font>
      <label>#AlbumInfo.Tracks</label>
      <scrollStartDelaySec>15</scrollStartDelaySec>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="250">WindowClose</animation>
      <visible>skin.hassetting(#AlbumInfo.ShowTracks)</visible>
    </control>
    <control>
      <type>group</type>
      <description>Button Group</description>
      <posX>33</posX>
      <posY>870</posY>
      <layout>StackLayout(0, Vertical, true)</layout>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="250">WindowClose</animation>
      <control Style="skinbutton">
        <description>Review/Tracks</description>
        <type>button</type>
        <id>5</id>
        <label>#(L(183))/#(L(182))</label>
        <width>405</width>
        <onleft>5</onleft>
        <onright>5</onright>
        <onup>6</onup>
        <ondown>6</ondown>
        <onclick>#(skin.togglesetting('#AlbumInfo.ShowTracks'))</onclick>
        <animation effect="fade" time="250">WindowOpen</animation>
        <animation effect="fade" time="250">WindowClose</animation>
      </control>
      <control Style="skinbutton">
        <description>Refresh</description>
        <type>button</type>
        <id>6</id>
        <label>184</label>
        <width>405</width>
        <onleft>6</onleft>
        <onright>6</onright>
        <onup>5</onup>
        <ondown>5</ondown>
        <animation effect="fade" time="250">WindowOpen</animation>
        <animation effect="fade" time="250">WindowClose</animation>
      </control>
    </control>
    <import>common.overlays2.xml</import>
