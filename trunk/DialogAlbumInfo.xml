      <animation effect="fade" time="250">WindowClose</animation>
    </control>
    <control>
      <description>Review</description>
      <type>textboxscrollup</type>
      <id>1</id>
      <posX>462</posX>
      <posY>435</posY>
      <width>1425</width>
      <height>322</height>
      <textcolor>ffFFFFFF</textcolor>
