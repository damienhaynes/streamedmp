      <description>Audio Channels / Codecs</description>
      <type>image</type>
      <id>9800</id>
      <posX>870</posX>
      <posY>10</posY>
      <width>100</width>
      <height>32</height>
      <texture>Logos\Audio\#MovingPictures.LocalMedia.audiocodec #MovingPictures.LocalMedia.audiochannels.png</texture>
      <visible>control.isvisible(50)+facadeview.list</visible>
      <animation effect="fade" time="250" delay="0">WindowClose</animation>
    </control>
    <control>
      <description>Audio Codecs - Unknown number of channels</description>
      <type>image</type>
      <id>0</id>
      <posX>870</posX>
      <posY>10</posY>
      <width>100</width>
      <height>32</height>
      <texture>Logos\Audio\#MovingPictures.LocalMedia.audiocodec.png</texture>
      <visible>control.isvisible(50)+facadeview.list+!Control.HasThumb(9800)</visible>
      <animation effect="fade" time="250" delay="0">WindowClose</animation>
    </control>
    <!-- Subtitles -->
    <control>
      <description>Subtitles</description>
      <type>image</type>
      <id>0</id>
      <posX>970</posX>
      <posY>10</posY>
      <width>100</width>
      <height>32</height>
      <texture>Logos\Misc\#MovingPictures.LocalMedia.Subtitles.png</texture>
      <visible>control.isvisible(50)+facadeview.list</visible>
      <animation effect="fade" time="250" delay="0">WindowClose</animation>
    </control>
  </controls>
</window>
