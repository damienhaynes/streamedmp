<?xml version="1.0" encoding="utf-8"?>
<!-- MediaInfo Logos (List and Filmstrip View) -->
<window>
  <controls>
    <!-- Video Resolutions / Aspect Ratio -->
    <control>
      <description>720p, 1080i, 1080p</description>
      <type>image</type>
      <id>9789</id>
      <posX>670</posX>
      <posY>10</posY>
      <width>100</width>
      <height>32</height>
      <texture>Logos\Video\#MovingPictures.LocalMedia.videoresolution.png</texture>
      <visible>control.isvisible(50)+facadeview.list</visible>
      <animation effect="fade" time="250" delay="0">WindowClose</animation>
    </control>
    <control>
      <description>SD/HD Fullscreen/Widescreen</description>
      <type>image</type>
      <id>0</id>
      <posX>670</posX>
      <posY>10</posY>
      <width>100</width>
      <height>32</height>
      <texture>Logos\Video\#MovingPictures.LocalMedia.videoresolution #MovingPictures.LocalMedia.videoaspectratio.png</texture>
      <visible>control.isvisible(50)+facadeview.list+!Control.HasThumb(9789)</visible>
      <animation effect="fade" time="250" delay="0">WindowClose</animation>
    </control>
    <!-- Video Codecs -->
    <control>
      <description>Video Codecs</description>
      <type>image</type>
      <id>0</id>
      <posX>770</posX>
      <posY>10</posY>
      <width>100</width>
      <height>32</height>
      <texture>Logos\Video\#MovingPictures.LocalMedia.videocodec.png</texture>
      <visible>control.isvisible(50)+facadeview.list</visible>
      <animation effect="fade" time="250" delay="0">WindowClose</animation>
    </control>
    <!-- Audio Channels / Codecs-->
