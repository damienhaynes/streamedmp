<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<window>
  <controls>
		<control>
      <description>Series and Season Posters</description>
      <type>group</type>
      <animation effect="fade" time="150">WindowOpen</animation>
      <animation effect="fade" time="250">WindowClose</animation>
      <animation effect="slide" time="150" start="1800,0">WindowOpen</animation>
      <animation effect="slide" time="250" end="1800,0">WindowClose</animation>
      <control>
        <type>facadeview</type>
        <id>50</id>
        <animation effect="fade" time="250" delay="0">WindowClose</animation>
        <control>
          <description>listcontrol</description>
          <type>listcontrol</type>
          <id>50</id>
          <posX>920</posX>
          <posY>360</posY>
          <height>720</height>
          <width>1000</width>
          <onleft>2</onleft>
          <onright>2</onright>
          <scrollOffset>5</scrollOffset>
          <itemWidth>48</itemWidth>
          <itemHeight>48</itemHeight>
          <IconXOff>37</IconXOff>
          <IconYOff>6</IconYOff>
          <textXOff>37</textXOff>
          <textYOff>7</textYOff>
          <textXOff2>0</textXOff2>
          <textYOff2>7</textYOff2>
          <textXOff3>112</textXOff3>
          <textYOff3>15</textYOff3>
          <font>mediastream12c</font>
          <font2>mediastream10c</font2>
          <font3>mediastream12tc</font3>
          <keepaspectratio>yes</keepaspectratio>
					<texturebg>-.png</texturebg>
					<lefttexture>-.png</lefttexture>
					<midtexture>osdprogressmid.png</midtexture>
					<righttexture>-.png</righttexture>
					<ProgressBarWidth>900</ProgressBarWidth>
					<ProgressBarHeight>3</ProgressBarHeight>
					<ProgressBarXOffset>90</ProgressBarXOffset>
					<ProgressBarYOffset>48</ProgressBarYOffset>
					<SelectedProgressOnly>No</SelectedProgressOnly>
          <spinPosX>1762</spinPosX>
          <spinPosY>1021</spinPosY>
          <spinAlign>Right</spinAlign>
          <spinColor>ff006698</spinColor>
          <textureUp>arrow_round_up_nofocus.png</textureUp>
          <textureDown>arrow_round_down_nofocus.png</textureDown>
          <textureUpFocus>arrow_round_up_focus.png</textureUpFocus>
          <textureDownFocus>arrow_round_down_focus.png</textureDownFocus>
          <spinWidth>75</spinWidth>
          <spinHeight>55</spinHeight>
          <suffix>|</suffix>
        </control>
        <control>
          <visible>control.isvisible(50)</visible>
          <description>Filmstrip</description>
          <type>filmstrip</type>
          <id>50</id>
          <onup>2</onup>
          <posX>60</posX>
          <posY>682</posY>
          <width>1920</width>
          <height>330</height>
          <scrollOffset>4</scrollOffset>
          <thumbWidth>172</thumbWidth>
          <thumbHeight>255</thumbHeight>
          <thumbPosX>3</thumbPosX>
          <thumbPosY>3</thumbPosY>
          <itemWidth>183</itemWidth>
          <itemHeight>138</itemHeight>
          <textureWidth>187</textureWidth>
          <textureHeight>114</textureHeight>
          <thumbPosX>5</thumbPosX>
          <thumbPosY>5</thumbPosY>
          <thumbs flipY="true" diffuse="diffusemirrorwide.png" />
          <InfoImage>
          </InfoImage>
          <imageFolder>-</imageFolder>
          <imageFolderFocus>-</imageFolderFocus>
          <frame>-</frame>
          <frameFocus>-</frameFocus>
          <showFrame>no</showFrame>
          <showFolder>yes</showFolder>
          <showBackGround>no</showBackGround>
          <showInfoImage>no</showInfoImage>
          <thumbAnimation effect="zoom" start="100,100" end="120,120" tween="quadratic" time="0">focus</thumbAnimation>
          <!-- Spin Controls -->
          <spinWidth>75</spinWidth>
          <spinHeight>55</spinHeight>
          <spinPosX>1762</spinPosX>
          <spinPosY>1021</spinPosY>
          <spinAlign>Right</spinAlign>
          <spinColor>ff006698</spinColor>
          <textureUp>arrow_round_up_nofocus.png</textureUp>
          <textureDown>arrow_round_down_nofocus.png</textureDown>
          <textureUpFocus>arrow_round_up_focus.png</textureUpFocus>
          <textureDownFocus>arrow_round_down_focus.png</textureDownFocus>
        </control>
        <control>
          <description>Thumbnail Panel</description>
          <type>thumbnailpanel</type>
          <id>50</id>
          <onleft>2</onleft>
          <onright>2</onright>
          <posX>695</posX>
          <posY>345</posY>
          <width>1225</width>
          <height>720</height>
          <!-- Small Icons 0.66 aspect ratio -->
          <itemWidth>150</itemWidth>
          <itemHeight>217</itemHeight>
          <textureWidth>150</textureWidth>
          <textureHeight>216</textureHeight>
          <thumbWidth>135</thumbWidth>
          <thumbHeight>202</thumbHeight>
          <thumbPosX>7</thumbPosX>
          <thumbPosY>7</thumbPosY>
          <!-- Large Icons 0.66 aspect ratio -->
          <textureWidthBig>222</textureWidthBig>
          <textureHeightBig>325</textureHeightBig>
          <itemWidthBig>220</itemWidthBig>
          <itemHeightBig>322</itemHeightBig>
          <thumbWidthBig>198</thumbWidthBig>
          <thumbHeightBig>300</thumbHeightBig>
          <thumbPosXBig>12</thumbPosXBig>
          <thumbPosYBig>12</thumbPosYBig>
          <imageFolderFocus>thumbborder3.png</imageFolderFocus>
          <font>font12</font>
          <suffix>|</suffix>
          <thumbZoom>yes</thumbZoom>
          <enableFocusZoom>no</enableFocusZoom>
          <!-- Spin Controls -->
          <spinWidth>75</spinWidth>
          <spinHeight>55</spinHeight>
          <spinPosX>1762</spinPosX>
          <spinPosY>1021</spinPosY>
          <spinAlign>Right</spinAlign>
          <spinColor>ff006698</spinColor>
          <textureUp>arrow_round_up_nofocus.png</textureUp>
          <textureDown>arrow_round_down_nofocus.png</textureDown>
          <textureUpFocus>arrow_round_up_focus.png</textureUpFocus>
          <textureDownFocus>arrow_round_down_focus.png</textureDownFocus>
        </control>
      </control>
    </control>
		<control>
  		<description>Poster</description>
  		<type>image</type>
  		<id>1</id>
  		<posX>80</posX>
  		<posY>360</posY>
  		<width>418</width>
  		<height>621</height>
  		<texture>#selectedthumb</texture>
  		<visible>![facadeview.filmstrip|facadeview.coverflow]+Control.IsVisible(50)</visible>
  		<animation effect="fade" time="250">WindowOpen</animation>
  		<animation effect="fade" time="250">WindowClose</animation>
  	</control>
	</controls>
</window>