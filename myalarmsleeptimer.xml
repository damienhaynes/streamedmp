<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<window>
  <id>5002</id>
  <defaultcontrol>3</defaultcontrol>
  <allowoverlay>yes</allowoverlay>
  <define>#header.label:850</define>
  <define>#header.image:-</define>
  <define>#header.hover:hover_my alarm.png</define>
  <define>#selecteditem:859</define>
  <define>#itemcount:-</define>
  <controls>
    <import>common.window.xml</import>
    <import>common.time.xml</import>

    <control>
      <type>group</type>
      <description>group element</description>
      <animation effect="slide" time="10" end="-300,0">WindowClose</animation>
      <animation effect="slide" time="10" start="-300,0">WindowOpen</animation>
      <posX>20</posX>
      <posY>80</posY>
      <layout>StackLayout</layout>
      <control>
        <description>SleepTimer On/Off button</description>
        <type>togglebutton</type>
        <id>3</id>
        <label>856</label>
        <onright>4</onright>
        <onup>17</onup>
        <ondown>7</ondown>
      </control>
      <control>
        <description>Reset Button</description>
        <type>button</type>
        <id>7</id>
        <label>868</label>
        <onright>4</onright>
        <onup>3</onup>
        <ondown>3</ondown>
      </control>
    </control>
    <control>
      <description>sleep time label</description>
      <type>label</type>
      <id>1</id>
      <posX>510</posX>
      <posY>245</posY>
      <label>#currentsleeptime</label>
      <align>left</align>
      <font>font72</font>
      <textcolor>20ffffff</textcolor>
    </control>
    <control>
      <description>label1</description>
      <type>label</type>
      <id>1</id>
      <posX>512</posX>
      <posY>129</posY>
      <label>865</label>
      <font>font13</font>
    </control>
    <control>
      <description>alarm time minutes</description>
      <type>spincontrol</type>
      <spintype>int</spintype>
      <orientation>vertical</orientation>
      <digits>2</digits>
      <id>4</id>
      <posX>948</posX>
      <posY>133</posY>
      <showrange>no</showrange>
      <onup>6</onup>
      <ondown>5</ondown>
      <onleft>3</onleft>
      <onright>5</onright>
    </control>
    <control>
      <description>fade out volume</description>
      <type>checkmark</type>
      <id>5</id>
      <posX>512</posX>
      <posY>174</posY>
      <width>266</width>
      <label>866</label>
      <onup>4</onup>
      <onleft>3</onleft>
      <onright>6</onright>
    </control>
    <control>
      <description>shutdown pc</description>
      <type>checkmark</type>
      <id>6</id>
      <posX>512</posX>
      <posY>220</posY>
      <width>266</width>
      <label>867</label>
      <onleft>3</onleft>
      <onup>5</onup>
      <ondown>4</ondown>
    </control>
  </controls>
</window>