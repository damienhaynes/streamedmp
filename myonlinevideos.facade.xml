<?xml version="1.0" encoding="utf-8"?>

<window>
  <controls>
    <control>
      <description>Selected item Background</description>
      <type>image</type>
      <id>0</id>
      <posX>0</posX>
      <posY>675</posY>
      <width>1280</width>
      <height>47</height>
      <texture>imagestreamtitle.png</texture>
      <animation effect="fade" time="300" delay="300">WindowOpen</animation>
      <animation effect="fade" time="230" delay="0">WindowClose</animation>
      <visible>!string.equals(#OnlineVideos.state, details)</visible>
    </control>
    <control>
      <description>Current SMS T9 Filter</description>
      <type>fadelabel</type>
      <id>1</id>
      <posX>25</posX>
      <posY>684</posY>
      <width>950</width>
      <font>mediastream16tc</font>
      <label>#OnlineVideos.filter</label>
      <visible>string.equals(#selectedindex,)</visible>
      <animation effect="fade" time="300" delay="300">WindowOpen</animation>
      <animation effect="fade" time="230" delay="0">WindowClose</animation>
    </control>
    <control>
      <description>Selected item Label</description>
      <type>fadelabel</type>
      <id>1</id>
      <posX>25</posX>
      <posY>684</posY>
      <width>950</width>
      <font>mediastream16tc</font>
      <label>#selecteditem</label>
      <visible>!string.equals(#selectedindex,)+!string.equals(#OnlineVideos.state, details)</visible>
      <animation effect="fade" time="300" delay="300">WindowOpen</animation>
      <animation effect="fade" time="230" delay="0">WindowClose</animation>
    </control>
    <control>
      <description>Page Count Background</description>
      <type>image</type>
      <id>0</id>
      <posX>1010</posX>
      <posY>683</posY>
      <width>260</width>
      <height>32</height>
      <texture>mediacountbg_Spin.png</texture>
      <visible>!string.equals(#OnlineVideos.state, details)</visible>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="250">WindowClose</animation>
      <animation effect="slide" time="150" start="800,0">WindowOpen</animation>
      <animation effect="slide" time="250" end="800,0">WindowClose</animation>
    </control>
    <control>
      <description>Page Count Label</description>
      <type>label</type>
      <label>30009</label>
      <id>0</id>
      <posX>1020</posX>
      <posY>688</posY>
      <font>mediastream12tc</font>
      <textcolor>ff909090</textcolor>
      <visible>!string.equals(#OnlineVideos.state, details)</visible>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="250">WindowClose</animation>
      <animation effect="slide" time="150" start="800,0">WindowOpen</animation>
      <animation effect="slide" time="250" end="800,0">WindowClose</animation>
    </control>
    <control>
      <description>Item Count Background</description>
      <type>image</type>
      <id>0</id>
      <posX>1113</posX>
      <posY>12</posY>
      <width>159</width>
      <height>27</height>
      <texture>mediacountbackground.png</texture>
      <visible>!string.equals(#OnlineVideos.state, details)</visible>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="250">WindowClose</animation>
    </control>
    <control>
      <type>label</type>
      <description>Number of Files Label</description>
      <id>1</id>
      <posX>1246</posX>
      <posY>14</posY>
      <width>350</width>
      <font>mediastream10c</font>
      <textcolor>ff006698</textcolor>
      <align>right</align>
      <label>#itemcount</label>
      <visible>!string.equals(#OnlineVideos.state, details)</visible>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="250">WindowClose</animation>
    </control>
    <control>
      <description>group element</description>
      <type>group</type>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="250">WindowClose</animation>
      <animation effect="slide" time="250" start="800,0">WindowOpen</animation>
      <animation effect="slide" time="250" end="800,0">WindowClose</animation>

      <!-- *** CLIPS LIST *** -->

     

      <control>
        <description>composite control consisting of a list control and a thumbnail panel</description>
        <type>facadeview</type>
        <id>50</id>
        <control>
          <description>List</description>
          <type>listcontrol</type>
          <id>50</id>
          <posX>620</posX>
          <posY>240</posY>
          <height>480</height>
          <width>660</width>
          <onleft>2</onleft>
          <onright>2</onright>
          <scrollOffset>2</scrollOffset>
          <PinIconWidth>15</PinIconWidth>
          <PinIconHeight>15</PinIconHeight>
          <PinIconXOff>57</PinIconXOff>
          <PinIconYOff>13</PinIconYOff>
          <itemWidth>42</itemWidth>
          <itemHeight>32</itemHeight>
          <IconXOff>12</IconXOff>
          <IconYOff>7</IconYOff>
          <textXOff>15</textXOff>
          <textYOff>5</textYOff>
          <textXOff2>0</textXOff2>
          <textYOff2>5</textYOff2>
          <font2>mediastream11</font2>
