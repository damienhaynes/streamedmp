          <textureUpFocus>arrow_round_up_focus.png</textureUpFocus>
          <textureDownFocus>arrow_round_down_focus.png</textureDownFocus>
          <spinWidth>50</spinWidth>
          <spinHeight>37</spinHeight>
          <suffix>|</suffix>
        </control>
        <control>
          <description>Thumbnail Panel</description>
          <type>thumbnailpanel</type>
          <id>50</id>
          <posX>510</posX>
          <posY>240</posY>
          <height>480</height>
          <width>770</width>
          <scrollOffset>0</scrollOffset>
          <onleft>2</onleft>
          <onright>2</onright>
          <itemWidth>188</itemWidth>
          <itemHeight>130</itemHeight>
          <textureWidth>171</textureWidth>
          <textureHeight>100</textureHeight>
          <thumbWidth>160</thumbWidth>
          <thumbHeight>90</thumbHeight>
          <thumbPosX>5</thumbPosX>
          <thumbPosY>5</thumbPosY>
          <itemWidthBig>375</itemWidthBig>
          <itemHeightBig>210</itemHeightBig>
          <textureWidthBig>315</textureWidthBig>
          <textureHeightBig>180</textureHeightBig>
          <thumbWidthBig>300</thumbWidthBig>
          <thumbHeightBig>170</thumbHeightBig>
          <thumbPosXBig>5</thumbPosXBig>
          <thumbPosYBig>5</thumbPosYBig>
          <zoomXPixels>0</zoomXPixels>
          <zoomYPixels>0</zoomYPixels>
          <thumbZoom>no</thumbZoom>
          <enableFocusZoom>no</enableFocusZoom>
          <keepaspectratio>yes</keepaspectratio>
          <hideUnfocusTexture>no</hideUnfocusTexture>
          <renderFocusText>yes</renderFocusText>
          <renderUnfocusText>yes</renderUnfocusText>
          <textYOff>0</textYOff>
          <playedColor>ff808080</playedColor>
          <spinPosX>1172</spinPosX>
          <spinPosY>681</spinPosY>
          <textureUp>arrow_round_up_nofocus.png</textureUp>
          <textureDown>arrow_round_down_nofocus.png</textureDown>
          <textureUpFocus>arrow_round_up_focus.png</textureUpFocus>
          <textureDownFocus>arrow_round_down_focus.png</textureDownFocus>
          <spinWidth>50</spinWidth>
          <spinHeight>37</spinHeight>
          <spinAlign>Right</spinAlign>
          <spinColor>ff006698</spinColor>
          <spinCanFocus>yes</spinCanFocus>
          <font>mediastream10c</font>
          <imageFolderFocus>thumbborder4.png</imageFolderFocus>
        </control>
        <control>
          <description>playlist listcontrol</description>
          <type>playlistcontrol</type>
          <id>50</id>
          <scrollOffset>2</scrollOffset>
          <spinPosX>1172</spinPosX>
          <spinPosY>681</spinPosY>
          <textureUp>arrow_round_up_nofocus.png</textureUp>
          <textureDown>arrow_round_down_nofocus.png</textureDown>
          <textureUpFocus>arrow_round_up_focus.png</textureUpFocus>
          <textureDownFocus>arrow_round_down_focus.png</textureDownFocus>
          <spinWidth>50</spinWidth>
          <spinHeight>37</spinHeight>
          <spinAlign>Right</spinAlign>
          <spinColor>ff006698</spinColor>
          <spinCanFocus>yes</spinCanFocus>
          <onleft>2</onleft>
          <onright>2</onright>
        </control>
      </control>
    </control>
  </controls>
</window>