<?xml version="1.0" encoding="utf-8"?>
<window>
  <controls>
    <control>
      <id>1</id>
      <description>Background Image</description>
      <type>image</type>
      <posX>0</posX>
      <posY>0</posY>
      <width>1280</width>
      <height>720</height>
      <texture>#header.hover</texture>
