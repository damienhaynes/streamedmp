<window>
  <id>6900</id>
  <defaultcontrol>6</defaultcontrol>
  <allowoverlay>yes</allowoverlay>
  <define>#header.label:wwiTV</define>
  <define>#header.image:-</define>
  <define>#header.hover:hover_wwiTV.png</define>
  <controls>
    <import>common.window.xml</import>
    <import>common.time.xml</import>
    <control>
      <description>Logo</description>
      <type>image</type>
      <id>2</id>
      <posX>1000</posX>
      <posY>0</posY>
      <width>200</width>
      <height>110</height>
      <texture>-</texture>
    </control>
    <control>
      <type>image</type>
      <id>1</id>
      <posX>0</posX>
      <posY>0</posY>
        <width>220</width>
        <height>720</height>
        <texture>streamed_submenu_bg.png</texture>
        <colorDiffuse>fffffffff</colorDiffuse>
      <animation effect="slide" time="10" end="-300,0">WindowClose</animation>
      <animation effect="slide" time="10" start="-300,0">WindowOpen</animation>
    </control>
    <control>
      <type>group</type>
      <description>group element</description>
      <animation effect="slide" time="10" end="-300,0">WindowClose</animation>
      <animation effect="slide" time="10" start="-300,0">WindowOpen</animation>
      <posX>20</posX>
      <posY>80</posY>
      <layout>StackLayout</layout>
      <control>
        <description>Select favorites</description>
        <type>button</type>
        <id>6</id>
        <label>932</label>
        <onleft>6</onleft>
        <onright>10</onright>
        <onup>50</onup>
        <ondown>4</ondown>
      </control>
      <control>
        <description>Select Country</description>
        <type>button</type>
        <id>4</id>
        <label>200026</label>
        <onleft>4</onleft>
        <onright>10</onright>
        <onup>6</onup>
        <ondown>9</ondown>
      </control>
      <control>
        <description>Select category</description>
        <type>button</type>
        <id>9</id>
        <label>2053</label>
        <onleft>9</onleft>
        <onright>10</onright>
        <onup>4</onup>
        <ondown>7</ondown>
      </control>
      <control>
        <description>Refresh data</description>
        <type>button</type>
        <id>7</id>
        <label>184</label>
        <onleft>7</onleft>
        <onright>10</onright>
        <onup>9</onup>
        <ondown>8</ondown>
      </control>
      <control>
        <description>Select language</description>
        <type>button</type>
        <id>8</id>
        <label>200025</label>
        <onleft>8</onleft>
        <onright>10</onright>
        <onup>7</onup>
        <ondown>99</ondown>
      </control>
    </control>
    <control>
      <description>Current category</description>
      <type>label</type>
      <id>5</id>
      <posX>35</posX>
      <posY>35</posY>
      <label>#Header</label>
    </control>
    <control>
      <description>Channels</description>
      <type>listcontrol</type>
      <id>10</id>
      <onleft>6</onleft>
      <onright>6</onright>
    </control>
    <control>
      <type>textboxscrollup</type>
      <description>Channel-Info</description>
      <id>11</id>
      <posX>310</posX>
      <posY>650</posY>
      <width>650</width>
      <height>94</height>
      <font>font12</font>
      <label>#ChannelInfo</label>
    </control>
  </controls>
</window>