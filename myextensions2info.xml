        <height>40</height>
        <onup>3</onup>
        <ondown>5</ondown>
        <onright>4</onright>
        <onleft>4</onleft>
        <textcolor>ffffffff</textcolor>
        <textcolorNoFocus>bfffffff</textcolorNoFocus>
