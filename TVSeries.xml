<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<window>
  <id>9811</id>
  <defaultcontrol>50</defaultcontrol>
  <allowoverlay>no</allowoverlay>
  <controls>
    <import>TVSeries.Common.Dummy.xml</import>
    <!-- Backgrounds -->
    <control>
      <description>Default Background</description>
      <type>image</type>
      <id>0</id>
      <posX>0</posX>
      <posY>0</posY>
      <width>1280</width>
      <height>720</height>
      <texture>hover_my tv series.png</texture>
      <animation effect="fade" time="250" delay="0">WindowOpen</animation>
    </control>
    <control>
      <description>Fanart Background #1</description>
      <type>image</type>
      <id>524</id>
      <posX>0</posX>
      <posY>0</posY>
      <width>1280</width>
      <height>720</height>
      <texture>#TVSeries.Fanart.1</texture>
      <animation effect="fade" time="300">visible</animation>
      <animation effect="fade" time="300" delay="300">hidden</animation>
    </control>
    <control>
      <description>Fanart Background #2</description>
      <type>image</type>
      <id>525</id>
      <posX>0</posX>
      <posY>0</posY>
      <width>1280</width>
      <height>720</height>
      <texture>#TVSeries.Fanart.2</texture>
      <animation effect="fade" time="300">visible</animation>
      <animation effect="fade" time="300" delay="300">hidden</animation>
    </control>
    <!-- Do not mix and match different styles (fanart/default) -->
    <!-- Chosen style should match in users StreamedMPConfig.xml -->
    <!-- Facade and SeriesWideBanners imports need to reflect widebanner mods -->
    <import tag="Facade">TVSeries.Default.Facade.xml</import>
    <import tag="SeriesAndSeasonListPosters">TVSeries.Default.SeriesAndSeasonListPosters.xml</import>
    <import tag="SeriesAndSeasonFilmstrip">TVSeries.Default.SeriesAndSeasonFilmstrip.xml</import>
    <import tag="SeriesListBanners">TVSeries.Default.SeriesListBanners.xml</import>
    <import tag="SeriesWideBanners">TVSeries.Default.SeriesWideBanners.xml</import>
    <import tag="EpisodeList">TVSeries.Default.EpisodeList.xml</import>
    <import tag="GroupList">TVSeries.Default.GroupList.xml</import>
    <!-- Common Imports -->
    <import>TVSeries.Coverflow.xml</import>
    <import>TVSeries.Common.HiddenMenu.xml</import>
    <import>common.time.xml</import>
    <import>common.overlays2.xml</import>
    <import>TVSeries.Common.TopBar.xml</import>
    <import>common.scrollingpopup.xml</import>
    <!-- IMPORT PROCESS ANIMATION -->
    <control>
      <description>Import Animation</description>
      <type>animation</type>
      <id>51</id>
      <posX>860</posX>
      <posY>15</posY>
      <width>157</width>
      <height>24</height>
      <textures>common.waiting.1.png;common.waiting.2.png;common.waiting.3.png;common.waiting.4.png;common.waiting.5.png;common.waiting.6.png;common.waiting.7.png;common.waiting.8.png;common.waiting.7.png;common.waiting.6.png;common.waiting.5.png;common.waiting.4.png;common.waiting.3.png;common.waiting.2.png</textures>
