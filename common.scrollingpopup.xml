<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<window>
  <controls>
    <control>
      <description>Scrolling Popup</description>
      <type>image</type>
      <id>1</id>
      <posX>892</posX>
      <posY>951</posY>
      <height>135</height>
      <width>135</width>
      <texture>popup.png</texture>
      <visible>facadeview.list+[string.contains(#scrolling.up,yes)|string.contains(#scrolling.down,yes)]+string.contains(#StreamedMP.ListScrollPopup,yes)+!string.equals(#selecteditem.scrolllabel,)</visible>
      <animation effect="slide" start="0,75" end="0,0" delay="250" time="150" reversible="true">visiblechange</animation>
    </control>
    <control>
      <description>Scrolling Label</description>
      <type>label</type>
      <id>0</id>
      <posX>960</posX>
      <posY>1020</posY>
      <label>#selecteditem.scrolllabel</label>
      <font>mediastream28tc</font>
      <align>center</align>
      <valign>middle</valign>
      <textcolor>ffFFFFFF</textcolor>
      <shadowAngle>45</shadowAngle>
      <shadowDistance>4</shadowDistance>
      <shadowColor>#shadow.light</shadowColor>
      <visible>facadeview.list+[string.contains(#scrolling.up,yes)|string.contains(#scrolling.down,yes)]+string.contains(#StreamedMP.ListScrollPopup,yes)</visible>
      <animation effect="slide" start="0,75" end="0,0" delay="250" time="150" reversible="true">visiblechange</animation>
    </control>
  </controls>
</window>
