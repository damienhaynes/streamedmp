		  <height>207</height>
		  <width>207</width>
		  <keepaspectratio>yes</keepaspectratio>
		  <zoom>yes</zoom>
		  <texture flipY="true" diffuse="diffusemirrorwide.png">#selectedthumb</texture>
		  <animation effect="fade" time="250" delay="0">WindowClose</animation>
		</control>
		<control>
		  <type>label</type>
		  <label>#Trakt.User.Username</label>
