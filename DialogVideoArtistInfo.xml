      <width>257</width>
      <height>380</height>
      <texture>defaultdvdcover.png</texture>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="250">WindowClose</animation>
    </control>
    <control>
      <description>Actor Thumb</description>
      <type>image</type>
      <id>21</id>
      <posX>31</posX>
      <posY>254</posY>
      <width>257</width>
      <height>380</height>
      <texture>#thumb</texture>
