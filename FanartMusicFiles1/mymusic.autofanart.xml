                <texture>#fanarthandler.music.backdrop1.any</texture>
                <visible>control.isvisible(91919297)+!string.equals(#fanarthandler.music.backdrop1.any,)+!string.equals(#fanarthandler.music.backdrop2.any,)+string.equals(#fanarthandler.music.backdrop1.play,)+string.equals(#fanarthandler.music.backdrop2.play,)</visible>
