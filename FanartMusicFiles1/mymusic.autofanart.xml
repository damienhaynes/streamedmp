                <timeperimage>30000</timeperimage>
                <fadetime>800</fadetime>
                <loop>yes</loop>
                <randomize>True</randomize>
                <visible>string.equals(#fanarthandler.music.backdrop1.any,) | string.equals(#fanarthandler.music.backdrop2.any,)</visible>
                <animation effect="fade" start="10" end="100" time="700" >Visible</animation>
                <animation effect="fade" start="100" end="10" time="700" delay="100">Hidden</animation>
         </control>
         
