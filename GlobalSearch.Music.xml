<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<window>
  <id>30885</id>
  <defaultcontrol>2</defaultcontrol>
  <allowoverlay>no</allowoverlay>
  <define>#header.label:#StreamedMP.SearchMusic</define>
  <define>#header.hover:hover_my music.png</define>
  <define>#usePlayFanart:Yes</define>
  <define>#useRandomMusicFanart:Yes</define>
  <controls>
    <import>mymusic.autofanart.xml</import>
    <control>
      <description>music overlay image</description>
      <animation effect="fade" start="100" end="0" time="250">WindowClose</animation>
      <type>image</type>
      <id>0</id>
      <posX>0</posX>
      <posY>0</posY>
      <width>1281</width>
      <height>720</height>
      <texture>mainoverlay.png</texture>
    </control>   
		<control>
      <type>group</type>
      <description>group element</description>
      <posX>50</posX>
      <posY>250</posY>
      <width>300</width>
      <height>80</height>
      <layout>StackLayout</layout>
      <control>
        <description>Search Phrase Button</description>
        <type>button</type>
        <id>2</id>
        <label>#StreamedMP.SearchPhrase</label>
        <onup>17</onup>
        <ondown>7</ondown>
        <onright>50</onright>
        <onleft>2</onleft>
        <textcolor>ffffffff</textcolor>
        <textcolorNoFocus>bfffffff</textcolorNoFocus>
        <font>mediastream12tc</font>
        <height>40</height>
        <width>300</width>
        <textYOff>8</textYOff>
        <textXOff>20</textXOff>
        <textureFocus>tv_button_focus.png</textureFocus>
        <textureNoFocus>tv_button_ no_focus.png</textureNoFocus>
        <animation effect="fade" time="250">WindowOpen</animation>
        <animation effect="fade" time="250">WindowClose</animation>
      </control>
      <control>
        <description>Search History Button</description>
        <type>button</type>
        <id>7</id>
        <label>#StreamedMP.SearchHistory</label>
        <onup>2</onup>
        <ondown>6</ondown>
        <onright>50</onright>
        <onleft>7</onleft>
        <textcolor>ffffffff</textcolor>
        <textcolorNoFocus>bfffffff</textcolorNoFocus>
        <font>mediastream12tc</font>
        <height>40</height>
        <textYOff>8</textYOff>
        <textXOff>20</textXOff>				
        <textureFocus>tv_button_focus.png</textureFocus>
        <textureNoFocus>tv_button_ no_focus.png</textureNoFocus>
        <animation effect="fade" time="250">WindowOpen</animation>
        <animation effect="fade" time="250">WindowClose</animation>
      </control>
			<control>
        <description>Search Fields Button</description>
        <type>button</type>
        <id>6</id>
        <label>#StreamedMP.SearchFields</label>
        <onup>7</onup>
        <ondown>5</ondown>
        <onright>50</onright>
        <onleft>6</onleft>
        <textcolor>ffffffff</textcolor>
        <textcolorNoFocus>bfffffff</textcolorNoFocus>
        <font>mediastream12tc</font>
        <height>40</height>
        <textYOff>8</textYOff>
        <textXOff>20</textXOff>
        <textureFocus>tv_button_focus.png</textureFocus>
        <textureNoFocus>tv_button_ no_focus.png</textureNoFocus>
        <animation effect="fade" time="250">WindowOpen</animation>
        <animation effect="fade" time="250">WindowClose</animation>
      </control>
			<control>
        <description>Search Type Select Button</description>
        <type>selectbutton</type>
        <id>5</id>
        <label>#StreamedMP.SearchType</label>
				<subitems>
					<subitem>Equals</subitem>
					<subitem>Starts with</subitem>
					<subitem>Contains</subitem>
					<subitem>Ends with</subitem>
        </subitems>
				<selected>2</selected>
        <onup>6</onup>
        <ondown>3</ondown>
        <onright>50</onright>
        <onleft>5</onleft>
        <textcolor>ffffffff</textcolor>
        <textcolorNoFocus>bfffffff</textcolorNoFocus>
        <font>mediastream12tc</font>
        <height>40</height>
        <textYOff>8</textYOff>
        <textXOff>20</textXOff>
				<texturebg>tv_button_focus.png</texturebg>
				<textureLeft>arrow_round_left_focus.png</textureLeft>
				<textureLeftFocus>arrow_round_left_nofocus.png</textureLeftFocus>
				<textureRight>arrow_round_right_nofocus.png</textureRight>
				<textureRightFocus>arrow_round_right_focus.png</textureRightFocus>
        <textureFocus>tv_button_focus.png</textureFocus>
        <textureNoFocus>tv_button_ no_focus.png</textureNoFocus>
        <animation effect="fade" time="250">WindowOpen</animation>
        <animation effect="fade" time="250">WindowClose</animation>
      </control>
			<control>
        <description>Case Sensitivity Button</description>
        <type>togglebutton</type>
        <id>3</id>
				<!-- Translations doesn't work? -->
        <!--<label>#StreamedMP.CaseSensitive</label>-->
				<label>Case Sensitive</label>
        <onup>5</onup>
        <ondown>2</ondown>
        <onright>50</onright>
        <onleft>3</onleft>
        <textcolor>ffffffff</textcolor>
        <textcolorNoFocus>bfffffff</textcolorNoFocus>
        <font>mediastream12tc</font>
        <height>40</height>
        <textYOff>8</textYOff>
        <textXOff>20</textXOff>
				<textureFocus>tv_button_focus_ledon.png</textureFocus>
        <textureNoFocus>tv_button_no_focus_ledon.png</textureNoFocus>
        <AltTextureFocus>tv_button_focus_ledoff.png</AltTextureFocus>
        <AltTextureNoFocus>tv_button_no_focus_ledoff.png</AltTextureNoFocus>
        <animation effect="fade" time="250">WindowOpen</animation>
        <animation effect="fade" time="250">WindowClose</animation>
      </control>
    </control>		
    <control>
      <type>image</type>
      <id>0</id>
      <posX>0</posX>
      <posY>675</posY>
      <width>1280</width>
      <height>47</height>
      <texture>imagestreamtitle.png</texture>
      <visible>true</visible>
			<animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="250">WindowClose</animation>
    </control>
    <control>
      <description>Page Count Background</description>
      <type>image</type>
      <id>0</id>
      <posX>1013</posX>
      <posY>684</posY>
      <width>260</width>
      <height>30</height>
      <texture>mediacountbg_Spin.png</texture>
      <visible>true</visible>
      <animation effect="fade" time="150">WindowOpen</animation>
      <animation effect="fade" time="250">WindowClose</animation>
      <animation effect="slide" time="150" start="800,0">WindowOpen</animation>
      <animation effect="slide" time="250" end="800,0">WindowClose</animation>
    </control>
    <control>
      <description>Page Count Label</description>
      <type>label</type>
      <label>30009</label>
      <id>0</id>
      <posX>1023</posX>
      <posY>688</posY>
      <font>mediastream12tc</font>
      <textcolor>ff909090</textcolor>
      <visible>true</visible>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="250">WindowClose</animation>
      <animation effect="slide" time="150" start="800,0">WindowOpen</animation>
      <animation effect="slide" time="250" end="800,0">WindowClose</animation>
    </control>
    <control>
      <description>Selected item Label</description>
      <type>fadelabel</type>
      <id>1</id>
      <posX>25</posX>
      <posY>684</posY>
      <width>950</width>
      <font>mediastream16tc</font>
      <label>#selecteditem</label>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="250">WindowClose</animation>
    </control>
    <control>
      <description>composite control consisting of a list control and a thumbnail panel</description>
      <type>facadeview</type>
      <id>50</id>
      <animation effect="fade" time="250">WindowOpen</animation>
      <animation effect="fade" time="250">WindowClose</animation>
      <animation effect="slide" time="150" start="800,0">WindowOpen</animation>
      <animation effect="slide" time="250" end="800,0">WindowClose</animation>
      <control>
        <description>listcontrol</description>
        <type>listcontrol</type>
        <id>50</id>
        <posX>520</posX>
        <posY>240</posY>
        <height>480</height>
        <width>760</width>
        <onleft>2</onleft>
        <onright>50</onright>
        <scrollOffset>5</scrollOffset>
        <PinIconWidth>15</PinIconWidth>
        <PinIconHeight>15</PinIconHeight>
        <PinIconXOff>57</PinIconXOff>
        <PinIconYOff>13</PinIconYOff>
        <itemWidth>32</itemWidth>
        <itemHeight>32</itemHeight>        
        <IconXOff>12</IconXOff>
        <IconYOff>4</IconYOff>
        <textXOff>25</textXOff>
        <textYOff>5</textYOff>
        <textXOff2>0</textXOff2>
        <textYOff2>5</textYOff2>        
        <keepaspectratio>no</keepaspectratio>        
        <spinPosX>1175</spinPosX>
        <spinPosY>681</spinPosY>
        <spinAlign>Right</spinAlign>
        <spinColor>ff006698</spinColor>
        <textureUp>arrow_round_up_nofocus.png</textureUp>
        <textureDown>arrow_round_down_nofocus.png</textureDown>
        <textureUpFocus>arrow_round_up_focus.png</textureUpFocus>
        <textureDownFocus>arrow_round_down_focus.png</textureDownFocus>
        <spinWidth>50</spinWidth>
        <spinHeight>37</spinHeight>
        <suffix>|</suffix>
      </control>
    </control>
    <import>common.window.xml</import>
    <import>common.time.xml</import>
    <import>common.overlays.xml</import>
  </controls>
</window>
