<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<window>
  <id>0</id>
  <defaultcontrol>50</defaultcontrol>
  <allowoverlay>no</allowoverlay>
  <autohidetopbar>yes</autohidetopbar>
  <define>#header.hover:hover_my plugins.png</define>
  <controls>
    <control>
      <type>image</type>
      <id>0</id>
      <posX>0</posX>
      <posY>0</posY>
      <width>1920</width>
      <height>1080</height>
      <texture>#header.hover</texture>
      <animation effect="fade" time="200" delay="300">WindowOpen</animation>
    </control>
    <control>
      <description>Menu</description>
      <type>menu</type>
      <id>50</id>
      <posX>825</posX>
      <posY>217</posY>
      <width>480</width>
      <textcolor>ffffffff</textcolor>
      <textColorNoFocus>ff505050</textColorNoFocus>
      <dimColor>ffffffff</dimColor>
      <buttonTextXOff>0</buttonTextXOff>
      <buttonTextYOff>0</buttonTextYOff>
      <buttonOffset>0</buttonOffset>
      <buttonFont>mediastream16tc</buttonFont>
      <buttonHeight>158</buttonHeight>
      <spaceBetweenButtons>0</spaceBetweenButtons>
      <numberOfButtons>5</numberOfButtons>
      <textureButtonFocus>-</textureButtonFocus>
      <textureButtonNoFocus>-</textureButtonNoFocus>
      <hoverX>0</hoverX>
      <hoverY>0</hoverY>
      <hoverWidth>1920</hoverWidth>
      <hoverHeight>1080</hoverHeight>
      <hoverKeepAspectratio>no</hoverKeepAspectratio>
      <scrollTime>90</scrollTime>
      <scrollTimeMin>45</scrollTimeMin>
      <mouseScrollTime>900</mouseScrollTime>
      <mouseScrollTimeMin>450</mouseScrollTimeMin>
      <textureBackground>-</textureBackground>
      <!--<onleft>17</onleft>-->
      <onright>10</onright>
      <animation effect="fade" time="400" delay="0">WindowOpen</animation>
    </control>
    <!-- Hidden Menu -->
    <control>
      <type>actiongroup</type>
      <description>action menu</description>
      <defaultcontrol>10</defaultcontrol>
      <onexit>50</onexit>
      <buttonX>1254</buttonX>
      <buttonY>426</buttonY>
      <buttonwidth>666</buttonwidth>
      <buttonheight>229</buttonheight>
      <textureFocus>-</textureFocus>
      <textureNoFocus>-</textureNoFocus>
      <posX>0</posX>
      <posY>0</posY>
      <width>384</width>
      <height>1080</height>
      <dimColor>00ffffff</dimColor>
      <control>
        <description>sub-blade</description>
        <type>image</type>
        <id>0</id>
        <posX>1254</posX>
        <posY>0</posY>
        <width>384</width>
        <height>1080</height>
        <texture>homebladesub.png</texture>
        <visible>!Control.HasFocus(50)</visible>
      </control>
      <control>
        <type>group</type>
        <description>group element</description>
        <visible>!Control.HasFocus(50)</visible>
        <animation effect="slide" time="0" start="0,0">visible</animation>
        <animation effect="slide" time="0" end="0,0">hidden</animation>
        <posX>1254</posX>
        <posY>405</posY>
        <width>384</width>
        <height>1080</height>
        <layout>StackLayout</layout>
        <control>
          <description>Shutdown button</description>
          <type>button</type>
          <id>10</id>
          <width>373</width>
          <height>75</height>
          <textXOff>37</textXOff>
          <textYOff>22</textYOff>
          <label>1030</label>
          <font>mediastream11tc</font>
          <action>99</action>
          <onup>14</onup>
          <ondown>11</ondown>
        </control>
        <control>
          <description>settings button</description>
          <type>button</type>
          <id>11</id>
          <width>373</width>
          <height>75</height>
          <textXOff>37</textXOff>
          <textYOff>22</textYOff>
          <label>1048</label>
          <font>mediastream11tc</font>
          <hyperlink>4</hyperlink>
          <onup>10</onup>
          <ondown>12</ondown>
        </control>
        <control>
          <description>videowallpaper button</description>
          <type>button</type>
          <id>12</id>
          <width>373</width>
          <height>75</height>
          <textXOff>37</textXOff>
          <textYOff>22</textYOff>
          <label>#StreamedMP.VideoWallpaperLabel</label>
          <font>mediastream11tc</font>
          <action>101</action>
          <onup>11</onup>
          <ondown>13</ondown>
        </control>
        <control>
          <description>basichome button button</description>
          <type>button</type>
          <id>13</id>
          <width>373</width>
          <height>75</height>
          <textXOff>37</textXOff>
          <textYOff>22</textYOff>
          <label>100035</label>
          <font>mediastream11tc</font>
          <action>115</action>
          <onup>12</onup>
          <ondown>14</ondown>
        </control>
        <control>
          <description>Exit button</description>
          <type>button</type>
          <id>14</id>
          <width>373</width>
          <height>75</height>
          <textXOff>37</textXOff>
          <textYOff>22</textYOff>
          <label>4517</label>
          <font>mediastream11tc</font>
          <action>97</action>
          <onup>13</onup>
          <ondown>10</ondown>
        </control>
      </control>
    </control>
    <control>
      <description>Homeblade</description>
      <type>image</type>
      <id>0</id>
      <posX>0</posX>
      <posY>0</posY>
      <width>1269</width>
      <height>1080</height>
      <texture>homebladeleft.png</texture>
      <colordiffuse>eaffffff</colordiffuse>
    </control>
    <control>
      <description>right overlay</description>
      <type>image</type>
      <id>0</id>
      <posX>1567</posX>
      <posY>0</posY>
      <width>352</width>
      <height>1080</height>
      <texture>homerightoverlay.png</texture>
    </control>
    <!-- Date -->
    <control>
      <description>Top Title Background</description>
      <type>image</type>
      <id>0</id>
      <posX>115</posX>
      <posY>16</posY>
      <width>655</width>
      <height>52</height>
      <texture>tvtitlebg.png</texture>
      <animation effect="slide" start="-771,0" end="0,0" tween="quadratic" time="300" delay="0">WindowOpen</animation>
    </control>
    <control>
      <type>fadelabel</type>
      <description>Top Title</description>
      <id>0</id>
      <posX>739</posX>
      <posY>19</posY>
      <width>525</width>
      <font>mediastream12c</font>
      <align>right</align>
      <visible>yes</visible>
      <label>#date</label>
      <animation effect="slide" start="-771,0" end="0,0" tween="quadratic" time="300" delay="0">WindowOpen</animation>
    </control>
    <!-- RSS Ticker -->
    <control>
      <description>Rss Background</description>
      <type>image</type>
      <id>0</id>
      <posX>112</posX>
      <posY>1017</posY>
      <width>1807</width>
      <height>63</height>
      <texture>homerss.png</texture>
      <visible>plugin.isenabled(InfoService)+!string.equals(#infoservice.feed.enabled,False)+!string.equals(#infoservice.feed.titles,)</visible>
      <animation effect="slide" start="0,150" end="0,0" tween="quadratic" easing="in" time=" 400" delay="200">WindowOpen</animation>
      <animation effect="slide" end="0,150" tween="quadratic" easing="in" time=" 400" delay="200">WindowClose</animation>
    </control>
    <control>
      <description>RSS Items</description>
      <type>fadelabel</type>
      <id>0</id>
      <width>1875</width>
      <height>75</height>
      <posX>171</posX>
      <posY>1033</posY>
      <font>mediastream12tc</font>
      <label>#infoservice.feed.titles</label>
      <wrapString> #infoservice.feed.separator </wrapString>
      <visible>plugin.isenabled(InfoService)+!string.equals(#infoservice.feed.enabled,False)</visible>
      <animation effect="slide" start="0,150" end="0,0" tween="quadratic" easing="in" time=" 400" delay="200">WindowOpen</animation>
      <animation effect="slide" end="0,150" tween="quadratic" easing="in" time=" 400" delay="200">WindowClose</animation>
    </control>
    <!-- imports -->
    <import>common.weather.topright.xml</import>
    <import>FritzManager.Notify.xml</import>
    <import>common.overlay.home.xml</import>
    <import>common.time.xml</import>
    <control>
      <description>Update Icon</description>
      <type>image</type>
      <id>0</id>
      <width>375</width>
      <height>375</height>
      <posX>1425</posX>
      <posY>375</posY>
      <align>center</align>
      <texture>skinupdate.png</texture>
      <visible>string.equals(#StreamedMP.UpdateAvailable,true)</visible>
      <animation effect="fade" delay="4000" start="100" end="0" time="3000">WindowOpen</animation>
      <animation effect="zoom" start="105,105" end="110,110" time="1500" tween="cubic" easing="inout" pulse="true" reversible="false" condition="true">Conditional</animation>
      <animation effect="fade" start="100" end="0" time="10000">Visible</animation>
      <animation effect="fade" delay="100" start="0" end="0" time="100">WindowClose</animation>
    </control>
  </controls>
</window>
