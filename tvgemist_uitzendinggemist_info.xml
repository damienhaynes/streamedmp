<window>
  <id>6801</id>
  <defaultcontrol>10</defaultcontrol>
  <allowoverlay>yes</allowoverlay>
  <define>#header.label:Uitzending Gemist</define>
  <define>#header.hover:hover_tvgemist.png</define>
  <define>#selecteditem:-</define>
  <define>#currentmodule:Uitzending gemist</define>
  <define>#extended:#title</define>
  <controls>
    <import>common.window.tvgemist.xml</import>
    <import>common.time.xml</import>
    <control>
      <type>group</type>
      <description>group element</description>
      <control>
        <type>image</type>
        <id>1</id>
        <posX>300</posX>
        <posY>400</posY>
        <width>276</width>
        <height>214</height>
        <texture>pic_preview_thumb_background.png</texture>
      </control>
      <control>
        <type>image</type>
        <description>Program image</description>
        <id>3</id>
        <posX>314</posX>
        <posY>414</posY>
        <width>245</width>
        <height>179</height>
        <texture>empty.png</texture>
      </control>
      <control>
        <description>Rating</description>
        <type>label</type>
        <id>0</id>
        <posX>40</posX>
        <posY>400</posY>
        <label>Rating</label>
        <textcolor>ffffffff</textcolor>
      </control>
      <control>
        <type>imagelist</type>
        <id>1</id>
        <posX>50</posX>
        <posY>430</posY>
        <width>120</width>
        <height>32</height>
        <textureWidth>24</textureWidth>
        <textureHeight>24</textureHeight>
        <subitems>
          <subitem>greystar.png</subitem>
          <subitem>starbest.png</subitem>
        </subitems>
        <imagesToDraw>5</imagesToDraw>
        <orientation>horizontal</orientation>
        <align>left</align>
        <percentage>#rating</percentage>
      </control>
      <control>
        <description>Genre</description>
        <type>label</type>
        <id>0</id>
        <posX>40</posX>
        <posY>460</posY>
        <label>Genre</label>
        <textcolor>ffffffff</textcolor>
      </control>
      <control>
        <description>Display which genre</description>
        <type>label</type>
        <id>4</id>
        <posX>50</posX>
        <posY>490</posY>
        <label>#genre</label>
        <textcolor>ff808080</textcolor>
      </control>
      <control>
        <description>Omroep</description>
        <type>label</type>
        <id>0</id>
        <posX>40</posX>
        <posY>520</posY>
        <label>Omroep</label>
        <textcolor>ffffffff</textcolor>
      </control>
      <control>
        <description>Display which omroep</description>
        <type>label</type>
        <id>5</id>
        <posX>50</posX>
        <posY>550</posY>
        <label>#credits</label>
        <textcolor>ff808080</textcolor>
      </control>
      <control>
        <type>textboxscroll</type>
        <description>Programma-info</description>
        <id>6</id>
        <posX>310</posX>
        <posY>100</posY>
        <width>800</width>
        <height>180</height>
        <label>#plot</label>
        <font>mediastream12tc</font>
        <textcolor>fffffff</textcolor>
      </control>
      <control>
        <description>afleveringen</description>
        <type>listcontrol</type>
        <id>10</id>
        <posY>400</posY>
        <spinPosY>660</spinPosY>
        <height>300</height>
        <onup>17</onup>
      </control>
      <control>
        <type>textboxscrollup</type>
        <description>Programma-info</description>
        <id>1</id>
        <posX>40</posX>
        <posY>250</posY>
        <width>1240</width>
        <height>150</height>
        <label>#plotoutline</label>
        <font>mediastream12tc</font>
        <textcolor>ffffffff</textcolor>
      </control>
    </control>
  </controls>
</window>
