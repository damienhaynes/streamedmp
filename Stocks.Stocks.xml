    <!-- *** Stock Item List *** -->
    <control>
      <description>listcontrol</description>
      <type>listcontrol</type>
      <id>60</id>
      <posX>310</posX>
      <posY>270</posY>
      <width>670</width>
      <height>350</height>
      <onleft>2</onleft>
      <font>font12</font>
      <textXOff>0</textXOff>
      <itemWidth>25</itemWidth>
      <itemHeight>25</itemHeight>
      <IconXOff>0</IconXOff>
      <IconYOff>8</IconYOff>
      <spinPosX>1175</spinPosX>
      <spinPosY>681</spinPosY>
      <spinAlign>Right</spinAlign>
      <spinColor>ff006698</spinColor>
      <textureUp>arrow_round_up_nofocus.png</textureUp>
      <textureDown>arrow_round_down_nofocus.png</textureDown>
      <textureUpFocus>arrow_round_up_focus.png</textureUpFocus>
      <textureDownFocus>arrow_round_down_focus.png</textureDownFocus>
      <spinWidth>50</spinWidth>
      <spinHeight>37</spinHeight>
      <animation effect="fade" time="300">WindowOpen</animation>
      <animation effect="fade" time="300">WindowClose</animation>
    </control>

    <!-- *** Stock Item Separator *** -->
    <control>
      <description>Separator</description>
      <type>label</type>
      <id>0</id>
      <posX>280</posX>
      <posY>587</posY>
      <width>1200</width>
      <label>____________________________________________________________________________________________________________</label>
      <textcolor>ff808080</textcolor>
      <visible>string.equals(#Stocks.StockListSelectable,true)+string.equals(#Stocks.Type,Stock)</visible>
      <animation effect="fade" time="300">WindowOpen</animation>
      <animation effect="fade" time="300">WindowClose</animation>
    </control>

    <!-- *** Stock Item Properties *** -->
    <control>
      <description>Stock Item Name</description>
      <type>label</type>
      <id>0</id>
      <posX>280</posX>
      <posY>580</posY>
      <width>830</width>
      <label>#Stocks.ListItemName</label>
      <visible>string.equals(#Stocks.StockListSelectable,true)+string.equals(#Stocks.Type,Stock)</visible>
      <animation effect="fade" time="300">WindowOpen</animation>
      <animation effect="fade" time="300">WindowClose</animation>
    </control>

    <control>
      <description>Stocks Item Date/Time</description>
      <type>label</type>
      <id>0</id>
      <posX>1270</posX>
      <posY>590</posY>
      <label>#Stocks.ListItemDate #Stocks.ListItemTime</label>
      <align>right</align>
      <visible>string.equals(#Stocks.StocksListSelectable,true)+string.equals(#Stocks.Type,Stock)</visible>
      <animation effect="fade" time="300">WindowOpen</animation>
      <animation effect="fade" time="300">WindowClose</animation>
    </control>
    <control>
      <description>Stock Item Last</description>
      <type>label</type>
      <id>0</id>
      <posX>280</posX>
      <posY>610</posY>
      <width>830</width>
      <label>Current Value: #Stocks.ListItemLast</label>
      <visible>string.equals(#Stocks.StockListSelectable,true)+string.equals(#Stocks.Type,Stock)+!string.equals(#Stocks.ListItemLast,)</visible>
      <animation effect="fade" time="300">WindowOpen</animation>
      <animation effect="fade" time="300">WindowClose</animation>
    </control>
    <control>
      <description>Stock Item Change</description>
      <type>label</type>
      <id>0</id>
      <posX>280</posX>
      <posY>630</posY>
      <width>830</width>
      <label>Change: #Stocks.ListItemChange (#Stocks.ListItemPercentChange)</label>
      <visible>string.equals(#Stocks.StockListSelectable,true)+string.equals(#Stocks.Type,Stock)+!string.equals(#Stocks.ListItemChange,)</visible>
      <animation effect="fade" time="300">WindowOpen</animation>
      <animation effect="fade" time="300">WindowClose</animation>
    </control>
    <control>
      <description>Stock Item Open</description>
      <type>label</type>
      <id>0</id>
      <posX>280</posX>
      <posY>650</posY>
      <width>830</width>
      <label>Close Value: #Stocks.ListItemClose, Open Value: #Stocks.ListItemOpen</label>
      <visible>string.equals(#Stocks.StockListSelectable,true)+string.equals(#Stocks.Type,Stock)+!string.equals(#Stocks.ListItemOpen,)</visible>
      <animation effect="fade" time="300">WindowOpen</animation>
      <animation effect="fade" time="300">WindowClose</animation>
    </control>
    <control>
      <description>Stock Item Low/High</description>
      <type>label</type>
      <id>0</id>
      <posX>280</posX>
      <posY>670</posY>
      <width>830</width>
      <label>Lowest/Highest Value: #Stocks.ListItemLow/#Stocks.ListItemHigh</label>
      <visible>string.equals(#Stocks.StockListSelectable,true)+string.equals(#Stocks.Type,Stock)+!string.equals(#Stocks.ListItemLow,)</visible>
      <animation effect="fade" time="300">WindowOpen</animation>
      <animation effect="fade" time="300">WindowClose</animation>
    </control>
    <control>
      <description>Stock Item Market</description>
      <type>label</type>
      <id>0</id>
      <posX>280</posX>
      <posY>690</posY>
      <width>830</width>
      <label>Market Capital: #Stocks.ListItemCapital (Volume: #Stocks.ListItemVolume, Ratio: #Stocks.ListItemRatio)</label>
      <visible>string.equals(#Stocks.StockListSelectable,true)+string.equals(#Stocks.Type,Stock)+!string.equals(#Stocks.ListItemCapital,)</visible>
      <animation effect="fade" time="300">WindowOpen</animation>
      <animation effect="fade" time="300">WindowClose</animation>
    </control>
  </controls>
</window>
