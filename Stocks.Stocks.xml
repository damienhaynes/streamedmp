      <posX>420</posX>
      <posY>945</posY>
      <width>1245</width>
      <label>Change: #Stocks.ListItemChange (#Stocks.ListItemPercentChange)</label>
      <visible>string.equals(#Stocks.StockListSelectable,true)+string.equals(#Stocks.Type,Stock)+!string.equals(#Stocks.ListItemChange,)</visible>
