      <posX>420</posX>
      <posY>915</posY>
      <width>1245</width>
      <label>Current Value: #Stocks.ListItemLast</label>
      <visible>string.equals(#Stocks.StockListSelectable,true)+string.equals(#Stocks.Type,Stock)+!string.equals(#Stocks.ListItemLast,)</visible>
      <animation effect="fade" time="300">WindowOpen</animation>
      <animation effect="fade" time="300">WindowClose</animation>
    </control>
    <control>
      <description>Stock Item Change</description>
      <type>label</type>
